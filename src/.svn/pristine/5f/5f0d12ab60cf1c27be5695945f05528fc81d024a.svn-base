<UserControl x:Class="SDN.Purchasing.View.DebitNoteListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             xmlns:local="clr-namespace:SDN.Purchasing.View"
               xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             mc:Ignorable="d" Width="1024"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
        <Grid Margin="10,0,10,10" Width="1004">
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
                <!--<SolidColorBrush x:Key="brushWatermarkForeground" Color="LightSteelBlue" />-->
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="0"></RowDefinition>
                <RowDefinition  Height="20"></RowDefinition>
                <RowDefinition  Height="Auto"></RowDefinition>
                <RowDefinition  Height="40"></RowDefinition>

            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <!--<StackPanel  Grid.Column="5" Grid.Row="0" Orientation="Vertical" HorizontalAlignment="Right" Margin="0 0 0 0">
                <Label  HorizontalAlignment="Right" Foreground="Black"  Margin="10,0,0,2" Content="Last Updated" />
                <TextBlock x:Name="LastUpdatedDate"  Margin="10,0,5,2" Grid.Row="0" HorizontalAlignment="Right"  Text="{Binding LastUpdateDate}"  />
                <Button x:Name="btnRefresh" Content="Refresh"  VerticalAlignment="Center" HorizontalAlignment="Right" Width="75"  Margin="0,4,0,4" Style="{DynamicResource ButtonStyle}" Command="{Binding RefreshCommand}"/>
            </StackPanel>-->
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.ColumnSpan="6" Grid.RowSpan="2" Grid.Row="0" Margin="0 15 0 10">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="0 2 10 20"   GroupName="radShowAll" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All "></TextBlock>
                        <!--<TextBlock  Text="{Binding ShowAllCount}" Margin="2,1,2,1"></TextBlock>-->
                        <TextBlock  Text=""></TextBlock>
                    </DockPanel>
                </RadioButton>
                <!--<RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>-->
            
            <Grid Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="1" Grid.RowSpan="2" MaxWidth="300" MinWidth="300" HorizontalAlignment="Left" Margin="20 0 0 10" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="0 0 5 0"
                        Command="{Binding YearQuarterSelectedCommand}"      IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"/>
                  <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>

            </Grid>
            <Grid Grid.Column="2" Grid.ColumnSpan="4" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Left" Margin="5 0 -15 5" MinWidth="300" MaxWidth="150" Width="282">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".30*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" Margin="20 0  5 0 " GroupName="radSelectionparameter" 
                      Command="{Binding CalendarSelectionCommand}"        IsChecked="{Binding  StartEndDateTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" Text="Start Date"></TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="End Date"></TextBlock>
                <DatePicker IsEnabled="{Binding EnableStartDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" Grid.Row="0"
                            PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"
                            Grid.Column="2" SelectedDate="{Binding SelectedSearchStartDate}"
                            >
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchStartDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>
                <DatePicker IsEnabled="{Binding EnableEndDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" 
                      PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"     DisplayDateStart="{Binding EndDateValidation}" Grid.Row="1" Grid.Column="2" SelectedDate="{Binding SelectedSearchEndDate}">
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchEndDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>
            </Grid>
                <TextBlock  Foreground="blue" FontStyle="Italic" Margin="13,0,0,20"><Run Text="To see Adjusted Debit Notes open Notifications."/></TextBlock>
            </StackPanel>
            <!--<Button Grid.Row="1" Grid.Column="5" HorizontalAlignment="Center" Style="{DynamicResource ButtonStyle}"
                    Command="{Binding SearchCommand}">Search</Button>-->
            <TextBlock Grid.Column="0" VerticalAlignment="Top" Grid.Row="4" Height="30" Margin="9,13,0,0"  
                  Style="{StaticResource WaterMarkStyle}"    Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbSuppNameSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbSuppNameSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="0" Grid.Row="4" IsEditable="True" Margin="2 10 0 0" HorizontalAlignment="Left" MaxHeight="30" VerticalAlignment="Top" Width="141" MaxWidth="141" MinWidth="141"
                      ItemsSource="{Binding DebitNoteListcmbSup}" 
                      DisplayMemberPath="SupplierName" SelectedValuePath="SupplierName" Name="cmbSuppNameSearch"
                      SelectedValue="{Binding SelectedSearchSupName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
            <ComboBox BorderBrush="Black" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="4" IsEditable="True" HorizontalAlignment="Left" Margin="144 10 0 0" MaxHeight="30" VerticalAlignment="Top" Width="97"  
                     Name="cmbStatus"
                      SelectedValue="{Binding SelectedSearchPI_Status ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Loaded="cmbStatus_Loaded">
                <ComboBoxItem >All</ComboBoxItem>
                <ComboBoxItem>Adjusted</ComboBoxItem>
                <ComboBoxItem>UnAdjusted</ComboBoxItem>
                <ComboBoxItem>Refunded</ComboBoxItem>
                
            </ComboBox>
            <TextBlock Grid.Column="2"  Style="{StaticResource WaterMarkStyle}"  VerticalAlignment="Top" Grid.Row="4" Height="30" Margin="-89 13 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbQuotNoSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbQuotNoSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="2" Grid.Row="4" IsEditable="True" HorizontalAlignment="Left" Margin="-93 10 0 0" MaxHeight="30" VerticalAlignment="Top" Width="100" 
                      ItemsSource="{Binding DebitNoteListcmbInv}" 
                      DisplayMemberPath="DebitNo" SelectedValuePath="DebitNo" Name="cmbQuotNoSearch"
                      SelectedValue="{Binding SelectedDebitNoteNo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
            <!--<ComboBox Grid.Column="5" Grid.Row="4" IsEditable="True" HorizontalAlignment="Right" Margin="100 20 18 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="300" MinWidth="153" Name="cmbConvertedtoSearch"
                      SelectedValue="{Binding SelectedSearchConvertedTo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
                <ComboBoxItem>PO</ComboBoxItem>
                <ComboBoxItem>PI</ComboBoxItem>

            </ComboBox>-->
            <TextBlock Grid.Column="3" VerticalAlignment="Top"  Style="{StaticResource WaterMarkStyle}"  Grid.Row="4" Height="30" Margin="70 13 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbDebitNoteNo" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbDebitNoteNo" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="3" Grid.Row="4" Grid.ColumnSpan="2"  IsEditable="True" HorizontalAlignment="Left" Margin="62 10 0 0" MaxHeight="30" VerticalAlignment="Top" 
                      Width="131"
                      ItemsSource="{Binding DebitNoteListcmbDebit}" 
                      DisplayMemberPath="InvoiceNoCashChequeNo" SelectedValuePath="InvoiceNoCashChequeNo" Name="cmbDebitNoteNo"
                      SelectedValue="{Binding SelectedSearchQNo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
            
            <StackPanel Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" Orientation="Horizontal" VerticalAlignment="Bottom" Margin="0 0 0 0">
                <TextBlock  Text="To create a Debit Note, create a Purchase Invoice total with minus value(Purchase Return), and Simple will automatically add a Debit Note " FontStyle="Italic"></TextBlock>
            </StackPanel>

            <!--<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="6" Margin="0,30,0,5" HorizontalAlignment="Left">-->
            <StackPanel  Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="6" Margin="0,40,10,0" >
                <Grid>
                    <DataGrid x:Name="CustomGridLines" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle41}" HeadersVisibility="All"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}" Width="140"></DataGridTextColumn>                            
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="97"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col2}"  Width="100"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col3}"  Width="103"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="123"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col5}"  Width="130"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col6}"  Width="142"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col7}"  Width="140"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid IsTextSearchEnabled="True" Width="990"  GridLinesVisibility="All" MinHeight="200" Height="{Binding DNGridHeight}" BorderThickness="1" x:Name="dg1" 
                     CanUserResizeColumns="False"   CanUserReorderColumns="False"  BorderBrush="Black"  HorizontalScrollBarVisibility="Disabled"   ItemsSource="{Binding DebitNoteList}" CanUserAddRows="False"  CanUserDeleteRows="False"
                      CanUserSortColumns="True"  SelectionMode="Single" VerticalAlignment="Top" 
                       IsReadOnly="True"   AutoGenerateColumns="False" Background="Transparent" 
                        ColumnHeaderStyle="{StaticResource HeaderStyle41}">
                  
                    <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="SupplierName" Width="140"
                                    SortMemberPath="SupplierName" Header="Supplier Name">
                            <DataGridTemplateColumn.HeaderTemplate>
                                <DataTemplate >                                   
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition>
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=SupplierName,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}">
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                            <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate >
                                        <TextBlock>
                                          <Hyperlink Command="{Binding DataContext.NavigateToClientCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding Sup_ID}">
                                                <TextBlock Text="{Binding SupplierName}" TextWrapping="Wrap" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn x:Name="Status" Width="97" MaxWidth="97" Header="Status" Binding="{Binding Path=StatusString}">
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition>
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Status,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}">
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                        </DataGridTextColumn>
                            <DataGridTemplateColumn x:Name="DebitNoteNo" Width="100" SortMemberPath="SortDebitNoteNo" HeaderStyle="{StaticResource HeaderStyle41Right}">
                               
                            <DataGridTemplateColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitNoteNo,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                       Debit Note No.
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text="" Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                            <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                          <Hyperlink Command="{Binding DataContext.NavigaetoPurchaseCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding DebitNo}" >   
                                                <TextBlock Text="{Binding DebitNo}" HorizontalAlignment="Right"/>
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn x:Name="DebitDate" Width="103" MaxWidth="103" HeaderStyle="{StaticResource HeaderStyle41Right}" 
                                            Binding="{Binding Path=DebitNoteDateStr}" SortMemberPath="DebitNoteDate">
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitDate,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                       Debit Note Date
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text="" Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>

                            <DataGridTextColumn x:Name="DebitAmount" Width="123" MaxWidth="123" Binding="{Binding Path=DebitNoteAmountStr}" SortMemberPath="DebitNoteAmount" HeaderStyle="{StaticResource HeaderStyle41Right}">
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitAmount,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                       Debit Note Amount
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text="" Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTemplateColumn x:Name="DebitCashNo" Width="130" MaxWidth="130" SortMemberPath="InvoiceNoCashChequeNo" HeaderStyle="{StaticResource HeaderStyle41Right}" >
                            <DataGridTemplateColumn.HeaderTemplate>
                                <DataTemplate>                                    
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitCashNo,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                         Purchase Invoice No.
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock HorizontalAlignment="Right" Text="Cash/Cheque No." Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                           
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                          <Hyperlink Command="{Binding DataContext.NavigaetoPurchaseInvoiceCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding InvoiceNoCashChequeNo}">
                                                <TextBlock Text="{Binding InvoiceNoCashChequeNo}" HorizontalAlignment="Right"/>
                                              
                                           </Hyperlink>
                                            
                                       </TextBlock>

                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn x:Name="DebitCashDate" Width="142" MaxWidth="142" Binding="{Binding Path=InvoiceCashChequeDateStr}"
                                                SortMemberPath="InvoiceCashChequeDate" HeaderStyle="{StaticResource HeaderStyle41Right}">
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>                                  
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitCashDate,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                            Purchase Invoice Date
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock HorizontalAlignment="Right" Text="Cash/Cheque Date" Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                               
                            </DataGridTextColumn>
                            <DataGridTextColumn HeaderStyle="{StaticResource HeaderStyle41Right}" x:Name="DebitCashAmount" Width="140" MaxWidth="140" Binding="{Binding Path=InvoiceCashChequeAmountStr}" SortMemberPath="InvoiceCashChequeAmount" >
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>                                    
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition ></RowDefinition>                                                
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DebitCashAmount,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock TextWrapping="Wrap" >
                                            Purchase Invoice Amount
                                                </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock HorizontalAlignment="Right" Text="Cash/Cheque Amount" Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                              
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                        
                    </DataGrid>
                </Grid>
                <StackPanel  Orientation="Horizontal" HorizontalAlignment="left" Margin="3,0,0,0" >
                    <Grid  Width="{Binding ElementName=SupplierName,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock  Text="Total" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>

                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid  Width="{Binding ElementName=Status,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid Width="{Binding ElementName=DebitNoteNo,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding ShowSelectedCount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1" >-->
                    <Grid Width="{Binding ElementName=DebitDate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" HorizontalAlignment="Right" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid Width="{Binding ElementName=DebitAmount,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalDebitAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}" ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=DebitCashNo,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=DebitCashDate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=DebitCashAmount,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text="{Binding TotalDebitCashAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}" ></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                </StackPanel>
            </StackPanel>
            <!--</ScrollViewer>-->
            <StackPanel Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="6" Orientation="Horizontal" VerticalAlignment="Bottom">
                <TextBlock  Text="Amounts Include  " FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Text="{Binding TaxName}" FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Margin="25 0 0 0"  Text="Amounts are in  " FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Text="{Binding CurrencyName}" FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Margin="25 0 0 0" Text="PS : Payment to Supplier" FontStyle="Italic" Foreground="Gray"></TextBlock>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="5" Grid.Row="5" HorizontalAlignment="Right"  Margin="10,10,13,0" VerticalAlignment="Bottom">
                <!--<Button Content="New"  Margin="10 0" Style="{DynamicResource ButtonStyle}" Command="{Binding NavigaetoPurchaseCommand}"/>-->
                <Button Content="Close"  Margin="10 0 0 0" Style="{DynamicResource ButtonStyle}"/>
            </StackPanel>
        </Grid>
    </ScrollViewer>
</UserControl>
