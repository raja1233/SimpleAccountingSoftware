<UserControl x:Class="SASClient.Accounts.Views.GstAndVatSummaryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SASClient.Accounts.Views"
             xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             mc:Ignorable="d" Width="1024"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid>
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
                <helpers:MultipleParameterConverter x:Key="MultiParamterConverter"></helpers:MultipleParameterConverter>
                <helpers:BoolToVisibilityConverter x:Key="BoolToVisConverter"></helpers:BoolToVisibilityConverter>
                <!--<SolidColorBrush x:Key="brushWatermarkForeground" Color="LightSteelBlue" />-->
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="350"></ColumnDefinition>
                <ColumnDefinition Width="400"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" Grid.RowSpan="3" MaxHeight="60" MaxWidth="300" MinWidth="300" HorizontalAlignment="Left" Margin="100,42,0,8" Width="305" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="0 0 5 0"
                       Command="{Binding YearQuarterSelectedCommand}"       IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}" HorizontalAlignment="Right" Width="92"></Border>
                <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>

            </Grid>
            <StackPanel Orientation="Horizontal" Grid.Column="0" Margin="7,40,256,10" Grid.RowSpan="2">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="2 2 10 2"   GroupName="radSelectionparameter" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All "></TextBlock>
                        <!--<TextBlock  Text="{Binding ShowAllCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>-->
                    </DockPanel>
                </RadioButton>
                <!--<RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>-->
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="0"  Grid.Row="0" Margin="0,-10,0,27" Grid.RowSpan="2">
                <RadioButton IsChecked="True" GroupName="radTax" Content="Tax Summary"  Style="{DynamicResource TabMenuButtonStyle}"  Grid.Column="0"  Margin="10,0,0,0"
                 KeyboardNavigation.AcceptsReturn="True"  Command="{Binding ShowTaxSummaryCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Content}"/>
                <RadioButton  GroupName="radTax" Content="Tax Collected Details"  Style="{DynamicResource TabMenuButtonStyle}"  Grid.Column="1"  Margin="10,0,0,0"
                  KeyboardNavigation.AcceptsReturn="True"   Command="{Binding ShowTaxCollectedCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Content}"/>
                <RadioButton  GroupName="radTax"  Content="Tax Paid Details"  Style="{DynamicResource TabMenuButtonStyle}"  Grid.Column="2" Margin="10,0,0,0"
                   KeyboardNavigation.AcceptsReturn="True"  Command="{Binding ShowTaxPaidDetailsCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Content}" />
                <!--<RadioButton x:Name="radExcgstYes" Command="{Binding ShowTaxSummaryCommand}"    GroupName="radTax" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding TaxSummaryTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Tax Summary "></TextBlock>
                        <TextBlock  Text="{Binding TaxName}" Margin="2,1,2,1"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radIncgstyes" Margin="15,0,2,2"  Command="{Binding ShowTaxCollectedCommand}"    GroupName="radTax" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding TaxCollectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Tax Collected Details "></TextBlock>
                        <TextBlock  Text="{Binding TaxName}" Margin="5,1,2,1"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radIncgstyes1"  Margin="15,0,2,2"  Command="{Binding ShowTaxPaidDetailsCommand}"    GroupName="radTax" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding TaxPaidDetailsTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Tax Paid Details"></TextBlock>
                        <TextBlock  Text="{Binding TaxName}" Margin="2,1,2,1"></TextBlock>

                    </DockPanel>
                </RadioButton>-->
            </StackPanel>
            <StackPanel  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" Margin="0,5,0,0" Width="980" >
                <Grid>
                    <!--gidline 1-->
                    <DataGrid x:Name="CustomGridLines" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding SIGridHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                           Visibility="{Binding Path=ShowSummaryTrue, Converter={StaticResource BoolToVisConverter}}"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="150"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="80"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="80"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="180"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="160"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="160"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="150"></DataGridTextColumn>

                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="5" MinHeight="100" Height="{Binding SIGridHeight}" IsTextSearchEnabled="True" GridLinesVisibility="All"  BorderThickness="1" x:Name="dg1" ItemsSource="{Binding GstAndVatSummaryEntityList}" CanUserAddRows="False"  CanUserDeleteRows="False" CanUserResizeColumns="False"
                     Visibility="{Binding Path=ShowSummaryTrue, Converter={StaticResource BoolToVisConverter}}" CanUserSortColumns="False"  ColumnHeaderStyle="{StaticResource HeaderStyle20}"    HorizontalScrollBarVisibility="Disabled" Background="Transparent"  CanUserReorderColumns="False" SelectionMode="Single"  VerticalAlignment="Top" AutoGenerateColumns="False" >
                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="TaxDescription" MinWidth="150" MaxWidth="150" Header=" Tax Description" Width="Auto" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                                <TextBlock Text="{Binding TaxDescriptionSummary}" HorizontalAlignment="Left"/>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxCode" MinWidth="80" MaxWidth="80" Header=" Tax Code" Width="Auto" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>                                        
                                                <TextBlock Text="{Binding TaxCodeSummary}" HorizontalAlignment="Left"/>
                                            <Hyperlink Command="{Binding DataContext.NavigateToProductCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding CashChequeNo}">
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxRate" MinWidth="80" MaxWidth="80"  Header=" Tax Rate"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                         
                                                <TextBlock Text="{Binding TaxRateSummary}" TextAlignment="Right" TextWrapping="Wrap"/>
                                         
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceCNAmount" MinWidth="180"  Header=" Invoice/CN Amount"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <TextBlock Text="{Binding TaxInvoiceCNAmountSummary}" HorizontalAlignment="Right" TextAlignment="Right" TextWrapping="Wrap"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxCollected" MinWidth="160" MaxWidth="160" Header=" Tax Collected" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                          
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <TextBlock Text="{Binding TaxCollectedSummary}" TextAlignment="Right" HorizontalAlignment="Right"/>
                                        <!--<Hyperlink Command="{Binding DataContext.NavigateToAccountCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding LinkedAccountID}">
                                                
                                           </Hyperlink>-->

                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceDNAmount" MinWidth="160"  Header=" Invoice/DN Amount"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding TaxInvoiceDNAmountSummary}" TextWrapping="Wrap" TextAlignment="Right" HorizontalAlignment="Right"/>

                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxPaid" MinWidth="150" MaxWidth="150"  Header="Tax Paid"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                           
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding TaxPaidSummary}" TextWrapping="Wrap" TextAlignment="Right" HorizontalAlignment="Right"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
                <StackPanel  Orientation="Horizontal"   HorizontalAlignment="left" Margin="0,1,0,0" Visibility="{Binding Path=ShowSummaryTrue, Converter={StaticResource BoolToVisConverter}}" >
                    <Grid  Width="{Binding ElementName=TaxDescription,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock  Text="Total" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>
                    <Grid  Width="{Binding ElementName=TaxCode,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=TaxRate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=InvoiceCNAmount,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalTaxInvoiceCNAmountSummary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>

                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=TaxCollected,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalTaxCollectedSummary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>

                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=InvoiceDNAmount,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalInvoiceDNAmountSummary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>

                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=TaxPaid,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text="{Binding TotalPaidSummary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>
                            <!--<TextBlock Text="{Binding TotalPrice,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>-->
                        </Border>
                    </Grid>

                </StackPanel>
                <StackPanel Margin="0 5 0 0" Visibility="{Binding Path=ShowSummaryTrue, Converter={StaticResource BoolToVisConverter}}">
                    <TextBlock Foreground="Gray" FontStyle="Italic" Text="CN: CreditNote"></TextBlock>
                    <TextBlock Foreground="Gray" Margin="100 -17 0 0" FontStyle="Italic" Text="DN: DebitNote"></TextBlock>
                    <TextBlock   Margin="200 -17 0 0"  Text="Amounts are in  " FontStyle="Italic" Foreground="Gray"></TextBlock>
                    <TextBlock  Text="{Binding CurrencyName}" Margin="305 -17 0 0" FontStyle="Italic" Foreground="Gray"></TextBlock>
                </StackPanel>
                <!--gridline1end-->
                <!--gridline2-->
                <Grid>
                    <DataGrid x:Name="CustomGridLines1" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding SIGridHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                           Visibility="{Binding Path=ShowTaxCollectedTrue, Converter={StaticResource BoolToVisConverter}}"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="120"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="100"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="90"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="130"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="120"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="130"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="130"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="140"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="5" MinHeight="100" Height="{Binding SIGridHeight}" IsTextSearchEnabled="True" GridLinesVisibility="All"  BorderThickness="1" x:Name="dg2" ItemsSource="{Binding GstAndVatCollectedEntityList}" CanUserAddRows="False"  CanUserDeleteRows="False" CanUserResizeColumns="False"
                    ColumnHeaderStyle="{StaticResource HeaderStyle20}" CanUserSortColumns="False" Visibility="{Binding Path=ShowTaxCollectedTrue, Converter={StaticResource BoolToVisConverter}}"   HorizontalScrollBarVisibility="Disabled" Background="Transparent"  CanUserReorderColumns="False" SelectionMode="Single"  VerticalAlignment="Top" AutoGenerateColumns="False" >
                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="TaxDescription1" MinWidth="120" MaxWidth="120" Header=" Tax Description" Width="Auto" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                           
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                                <TextBlock Text="{Binding TaxDescriptionC}" />
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxCode1" MinWidth="100" MaxWidth="100" Header=" Tax Code" Width="Auto" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                           
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>                                        
                                                <TextBlock Text="{Binding TaxCodeC}" />
                                             <Hyperlink Command="{Binding DataContext.NavigateToProductCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding CashChequeNo}">
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TaxRate1" MinWidth="90" MaxWidth="90"  Header=" Tax Rate"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                          
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding TaxRateC}" TextWrapping="Wrap" TextAlignment="Right" HorizontalAlignment="Right" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="CustomerName1" MinWidth="130"  Header=" Customer Name" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                          
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Left">
                                        <Hyperlink  Command="{Binding DataContext.NavigateToCustomerName, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                            >

                                            <InlineUIContainer>
                                                <TextBlock HorizontalAlignment="Right" Text="{Binding CustomersName}" />
                                            </InlineUIContainer>
                                            <Hyperlink.CommandParameter>
                                                <MultiBinding Converter="{StaticResource MultiParamterConverter}">
                                                    <Binding Path="ID"></Binding>
                                                    <Binding Path="TransactionType"></Binding>
                                                </MultiBinding>
                                            </Hyperlink.CommandParameter>
                                        </Hyperlink>
                                        </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceCNNumber1" MinWidth="120" MaxWidth="120" Header=" Invoice/CN No."   Width="Auto">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                           
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                           <Hyperlink Command="{Binding DataContext.NavigateToInvoiceNumber, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding TaxInvoiceNoC}">
                                                <TextBlock Text="{Binding TaxInvoiceNoC}" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceCNDate1" MinWidth="130"  Header=" Invoice/CN Date"  HeaderStyle="{StaticResource HeaderStyle20Right}" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
