<UserControl x:Class="SASClient.Accounts.Views.AccountsTransactionsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SASClient.Accounts.Views"
             xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             mc:Ignorable="d" Width="1024"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid >
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
                <helpers:MultipleParameterConverter x:Key="MultiParamterConverter"></helpers:MultipleParameterConverter>
                <!--<SolidColorBrush x:Key="brushWatermarkForeground" Color="LightSteelBlue" />-->
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>

            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="212"></ColumnDefinition>
                <!--<ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>-->
            </Grid.ColumnDefinitions>
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" Margin="0 0 0 0">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="2 2 10 2"   GroupName="radShowAll" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All ("></TextBlock>
                        <TextBlock  Text="{Binding ShowAllCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>
            </StackPanel>
            <Grid Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Grid.RowSpan="2" MaxWidth="300" MinWidth="300" HorizontalAlignment="Left" Margin="0 0 0 0" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="2 0 5 0"
                       Command="{Binding YearQuarterSelectedCommand}"       IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>

            </Grid>
            <Grid Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Left" Margin="60 0 0 0" MinWidth="300" MaxWidth="600">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" Margin="20 0  5 0 " GroupName="radSelectionparameter" 
                        Command="{Binding CalendarSelectionCommand}"     IsChecked="{Binding  StartEndDateTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" Text="Start Date"></TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="End Date"></TextBlock>

                <DatePicker IsEnabled="{Binding EnableStartDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" Grid.Row="0"
                           PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"
                            Grid.Column="2" SelectedDate="{Binding SelectedSearchStartDate}" >
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox  x:Name="PART_TextBox"  IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchStartDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>

                <DatePicker IsEnabled="{Binding EnableEndDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" 
                      PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"     DisplayDateStart="{Binding EndDateValidation}" Grid.Row="1" Grid.Column="2" SelectedDate="{Binding SelectedSearchEndDate}">
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchEndDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>

            </Grid>

            <StackPanel   
                         Grid.Row="3" Grid.Column="0"  Margin="10,40,10,0">
                <Grid Margin="0,0,-62,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50*"/>
                        <ColumnDefinition Width="50*"/>

                    </Grid.ColumnDefinitions>
                    <!--<DataGrid x:Name="CustomGridLines2" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,-424,0" Grid.ColumnSpan="7"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="150"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="150"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>-->
                    <TextBlock Margin="0,-25,-113,0" Grid.ColumnSpan="7">Click on any Account Name to See Transactions Details</TextBlock>

                    <Grid  Grid.ColumnSpan="2" Width="340">
                        <DataGrid x:Name="CustomGridLines2" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0"  >
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="140"></DataGridTextColumn>
                                <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="200"></DataGridTextColumn>

                            </DataGrid.Columns>
                        </DataGrid>
                        <DataGrid Grid.ColumnSpan="2" IsTextSearchEnabled="True"   GridLinesVisibility="All" IsReadOnly="True" BorderThickness="1" 
                  HorizontalScrollBarVisibility="Disabled"  SelectedValuePath="ProductCode" MinHeight="100"  Height="{Binding SIGridHeight}"  x:Name="dg2" ItemsSource="{Binding AccountsTransactionNameTypeList}" CanUserAddRows="False" 
                  ColumnHeaderStyle="{StaticResource HeaderStyle20}" SelectedValue="{Binding ProductCode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Background="Transparent"  CanUserDeleteRows="False" SelectionMode="Single" VerticalAlignment="Top" AutoGenerateColumns="False"  >

                            <DataGrid.Columns>
                                <DataGridTemplateColumn x:Name="AccountType" Width="140" MaxWidth="140" Header="Account Type" >
                                    <DataGridTemplateColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid Width="182">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition></RowDefinition>

                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <!--<ColumnDefinition Width="100" />
                                                    <ColumnDefinition  Width="10"/>-->
                                                    <ColumnDefinition Width="{Binding ElementName=AccountType,Path=ActualWidth}"/>
                                                </Grid.ColumnDefinitions>
                                                <DockPanel Grid.Row="0" Grid.Column="0">
                                                    <TextBlock Text="{TemplateBinding Content}"  />
                                                    <!--<Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>-->
                                                </DockPanel>

                                            </Grid>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.HeaderTemplate>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock>
                                                  <TextBlock Text="{Binding AccountType}" HorizontalAlignment="Left"/></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn x:Name="AccountName" Width="200" MaxWidth="200" Header="Account Name" >
                                    <DataGridTemplateColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid Width="182">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition></RowDefinition>

                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <!--<ColumnDefinition Width="auto" />
                                                    <ColumnDefinition  Width="10"/>-->
                                                    <ColumnDefinition Width="{Binding ElementName=AccountName,Path=ActualWidth}"/>
                                                </Grid.ColumnDefinitions>
                                                <DockPanel Grid.Row="0" Grid.Column="0">
                                                    <TextBlock Text="{TemplateBinding Content}"  />
                                                    <!--<Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>-->
                                                </DockPanel>

                                            </Grid>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.HeaderTemplate>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock>
                                                  <TextBlock Text="{Binding AccountName}" HorizontalAlignment="Left"/></TextBlock>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>
                            <!--<DataGrid.Resources>
                                <Style TargetType="DataGridRow">
                                    <Style.Resources>
                                        <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Black"/>
                                    </Style.Resources>
                                </Style>
                            </DataGrid.Resources>-->

                            <!--<i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding SelectChangedCommand}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>-->
                            <DataGrid.CellStyle>
                                <Style TargetType="DataGridCell">
                                    <Style.Triggers>
                                        <Trigger Property="IsSelected"
                                                     Value="True">
                                            <Setter Property="BorderBrush"
                                                 Value="Black" />
                                            <Setter Property="Background"
                                                    Value="Black" />
                                            <Setter Property="Foreground"
                                                 Value="White" />

                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGrid.CellStyle>
                            <DataGrid.RowStyle>
                                <Style TargetType="DataGridRow">
                                    <Style.Triggers>
                                        <Trigger Property="IsSelected"
                                                     Value="True">
                                            <Setter Property="BorderBrush"
                                                 Value="Black" />
                                            <Setter Property="Background"
                                                    Value="Black" />
                                            <Setter Property="Foreground"
                                                 Value="White" />

                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGrid.RowStyle>
                        </DataGrid>
                    </Grid>
                </Grid>

            </StackPanel>
            <!--<ScrollViewer VerticalScrollBarVisibility="Auto"  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" Margin="-10,30,0,0">-->
            <StackPanel   
                         Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3" Margin="80,40,10,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="37*"/>
                        <ColumnDefinition Width="215*"/>
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="CustomGridLines1" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0" Grid.ColumnSpan="3"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="100"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="200"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col2}"  Width="145"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col3}"  Width="100"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="80"></DataGridTextColumn>
                            <!--<DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="80"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="80"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="80"></DataGridTextColumn>-->
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="3" IsTextSearchEnabled="True"   GridLinesVisibility="All" 
                  HorizontalScrollBarVisibility="Disabled" MinHeight="100"  Height="{Binding SIGridHeight}"  BorderThickness="1" x:Name="dg1" ItemsSource="{Binding StockCardListProductDetails}" CanUserAddRows="False" 
                  ColumnHeaderStyle="{StaticResource HeaderStyle20}"  Background="Transparent"  CanUserDeleteRows="False" SelectionMode="Single" VerticalAlignment="Top" AutoGenerateColumns="False"  >
                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="TransactionType" Width="100" MaxWidth="100" Header="Trans Type" SortMemberPath="TransactionType" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid Width="182">
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <!--<ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>-->
                                                <ColumnDefinition Width="{Binding ElementName=TransactionType,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                             />
                                            </DockPanel>

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                                  <TextBlock Text="{Binding TransactionType}" /></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Name" MinWidth="200"  MaxWidth="200" Header="Cus/Sup/P&amp;S/Acc Name"  Width="Auto" SortMemberPath="Name">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Name,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}"  HorizontalAlignment="Right"/>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Left"><Hyperlink Command="{Binding DataContext.NavigateToClientCommand, RelativeSource={RelativeSource
                                            AncestorType={x:Type UserControl}}}" >
                                            <InlineUIContainer>
                                                 <TextBlock HorizontalAlignment="Left" Text="{Binding Name}" />
                                             </InlineUIContainer>
                                            <Hyperlink.CommandParameter>
                                            <MultiBinding Converter="{StaticResource MultiParamterConverter}">
                                                 <Binding Path="ID"/>
                                                 <Binding Path="TransactionType"/>
                                             </MultiBinding>
                                        </Hyperlink.CommandParameter>
                                        </Hyperlink>
                                      </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TransactionNo" MinWidth="145" MaxWidth="145" Header="Transaction No."  Width="Auto" SortMemberPath="TransactionNo"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=TransactionNo,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}" HorizontalAlignment="Right" />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1" />
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right"><Hyperlink CommandParameter="{Binding TransactionNo}" Command="{Binding DataContext.NavigaetoStockCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                ><InlineUIContainer>   
                                                  <TextBlock HorizontalAlignment="Right" Text="{Binding TransactionNo}" />
                                              </InlineUIContainer>
                                        </Hyperlink></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Date" MinWidth="100" MaxWidth="100" Header="Date"  Width="Auto" SortMemberPath="TransactionDate" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DateTime,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding InvoiceDate}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Amount" MinWidth="80" MaxWidth="80" Header="Amount"  Width="Auto" SortMemberPath="TransactionDate" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Amount,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding Amount}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <!--<DataGridTemplateColumn x:Name="QtyIn" MinWidth="80" MaxWidth="80" Header="Qty In"  Width="Auto" SortMemberPath="QtyIn" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DateTime,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding QtyIn}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="QtyOut" MinWidth="80" MaxWidth="80" Header="Qty Out"  Width="Auto" SortMemberPath="TransactionDate" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
