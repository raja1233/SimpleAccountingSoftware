<UserControl x:Class="SDN.Purchasing.View.PurchaseInvoiceListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             xmlns:local="clr-namespace:SDN.Purchasing.View"
                xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             mc:Ignorable="d" Width="1024"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid Margin="10,0,10,10" Width="1004">
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter  x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
              
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="0"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <!--<ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>-->
            </Grid.ColumnDefinitions>

            <!--<StackPanel  Grid.Column="5" Grid.Row="0" Orientation="Vertical" HorizontalAlignment="Right" Margin="0 0 0 0">
                <Label  HorizontalAlignment="Right" Foreground="Black"  Margin="10,0,0,2" Content="Last Updated" />
                <TextBlock x:Name="LastUpdatedDate"  Margin="10,0,5,2" Grid.Row="0" HorizontalAlignment="Right"  Text="{Binding LastUpdateDate}"  />
                <Button x:Name="btnRefresh" Content="Refresh"  VerticalAlignment="Center" HorizontalAlignment="Right" Width="75"  Margin="0,4,0,4" Style="{DynamicResource ButtonStyle}" Command="{Binding RefreshCommand}"/>
            </StackPanel>-->
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.ColumnSpan="6" Grid.RowSpan="2" Grid.Row="0" Margin="0 15 0 10">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="0 2 10 20"   GroupName="radShowAll" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All "></TextBlock>
                        <!--<TextBlock  Text="{Binding ShowAllCount}" Margin="2,1,2,1"></TextBlock>-->
                        <TextBlock  Text=""></TextBlock>
                    </DockPanel>
                </RadioButton>
                <!--<RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>-->

                <Grid Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="1" Grid.RowSpan="2" MaxWidth="300" MinWidth="300" HorizontalAlignment="Left" Margin="15 0 0 10" >
                    <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="0 0 5 0"
                        Command="{Binding YearQuarterSelectedCommand}"      IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>

            </Grid>
                <Grid Grid.Column="2" Grid.ColumnSpan="4" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Left" Margin="5 0 -10 5" MinWidth="300" MaxWidth="150" Width="282">
                    <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".30*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" Margin="20 0  5 0 " GroupName="radSelectionparameter" 
                         Command="{Binding CalendarSelectionCommand}"     IsChecked="{Binding  StartEndDateTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" Text="Start Date"></TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="End Date"></TextBlock>
                <DatePicker IsEnabled="{Binding EnableStartDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" Grid.Row="0"
                            PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"
                            Grid.Column="2" SelectedDate="{Binding SelectedSearchStartDate}"
                            >
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchStartDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>
                <DatePicker IsEnabled="{Binding EnableEndDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" 
                      PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"     DisplayDateStart="{Binding EndDateValidation}" Grid.Row="1" Grid.Column="2" SelectedDate="{Binding SelectedSearchEndDate}">
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchEndDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>
            </Grid>
                <TextBlock Foreground="Black" FontStyle="Italic" Margin="5,0,0,20">
               To see Payment Due Dates open
            </TextBlock>
                <TextBlock FontStyle="Italic" Margin="5 0 0 0" HorizontalAlignment="Right"><Hyperlink Command="{Binding DataContext.NavigateToDoListCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}">To Do List   
                </Hyperlink></TextBlock>
            </StackPanel>
            <!--<Button Grid.Row="1" Grid.Column="5" HorizontalAlignment="Center" Style="{DynamicResource ButtonStyle}"
                    Command="{Binding SearchCommand}">Search</Button>-->
            <TextBlock Grid.Column="0" VerticalAlignment="Top" Grid.Row="3" Height="30" Margin="10,5,0,0"  
                 Style="{StaticResource WaterMarkStyle}"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbSuppNameSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbSuppNameSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox  Grid.Column="0" Grid.Row="3" IsEditable="True" Margin="2 0 0 0" HorizontalAlignment="Left" MaxHeight="30" VerticalAlignment="Top" MaxWidth="179" MinWidth="179"
                      ItemsSource="{Binding PurchaseInvoiceListcmbSup}" Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"
                      DisplayMemberPath="SupplierName" SelectedValuePath="SupplierName" Name="cmbSuppNameSearch"
                      SelectedValue="{Binding SelectedSearchSupName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
            <ComboBox BorderBrush="Black" Grid.Column="1" Grid.Row="3" IsEditable="True" HorizontalAlignment="Left" Margin="-69 0 0 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="86" MinWidth="86"
                     Name="cmbStatus" 
                      SelectedValue="{Binding SelectedSearchPI_Status ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Loaded="cmbStatus_Loaded">
                <ComboBoxItem >All</ComboBoxItem>
                <ComboBoxItem>Paid</ComboBoxItem>
                <ComboBoxItem>Unpaid</ComboBoxItem>
                <ComboBoxItem>Adjusted</ComboBoxItem>
                <ComboBoxItem>Cancelled</ComboBoxItem>
            </ComboBox>
            <TextBlock Grid.Column="1" VerticalAlignment="Top" Grid.Row="3" Height="30" Margin="27 5 0 0" 
                Style="{StaticResource WaterMarkStyle}" Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbQuotNoSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbQuotNoSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox  Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="1" Grid.Row="3" IsEditable="True" HorizontalAlignment="Left" Margin="18 0 0 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="103" MinWidth="103"
                      ItemsSource="{Binding PurchaseInvoiceListcmbInv}" 
                      DisplayMemberPath="InvoiceNo" SelectedValuePath="InvoiceNo" Name="cmbQuotNoSearch"
                      SelectedValue="{Binding SelectedSearchSupName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
           
            <TextBlock Grid.Column="2" VerticalAlignment="Top" Grid.Row="3" Height="30" Margin="92 5 0 0"
                       Style="{StaticResource WaterMarkStyle}"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbDebitNoteNo" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbDebitNoteNo" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="2" Grid.Row="3"  IsEditable="True" HorizontalAlignment="Left" Margin="87 0 0 0" MaxHeight="30"
                      VerticalAlignment="Top" MaxWidth="118" MinWidth="118"
                      ItemsSource="{Binding PurchaseInvoiceListcmbDebit}" 
                      DisplayMemberPath="CashDebitNo" SelectedValuePath="DebitNoteNo" Name="cmbDebitNoteNo"
                      SelectedValue="{Binding SelectedDebitNoteNo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" >

                </ComboBox>

            <!--<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" Margin="-10,30,0,5">
                <StackPanel Orientation="Horizontal" Margin="5,5,5,5" MaxWidth="1000" MaxHeight="450">-->
            <StackPanel Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4"  Margin="0,30,2,0">
                <Grid>
                <DataGrid x:Name="CustomGridLines" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle41}" HeadersVisibility="All"  >
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="" Binding="{Binding Col0}" Width="180"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="86"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col2}"  Width="105"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col3}"  Width="100"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="115"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col5}"  Width="118"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col6}"  Width="130"></DataGridTextColumn>
                        <DataGridTextColumn Header="" Binding="{Binding Col7}"  Width="146"></DataGridTextColumn>
                    </DataGrid.Columns>
                       
                    </DataGrid>
                
                    <DataGrid Background="Transparent"  IsTextSearchEnabled="True" Width="997" ScrollViewer.VerticalScrollBarVisibility="Visible"
                          HorizontalScrollBarVisibility="Hidden"   Height="{Binding PIGridHeight}"  Grid.Column="0"  
                      GridLinesVisibility="All"  BorderThickness="1" x:Name="dg1" ItemsSource="{Binding PurchaseInvoiceList}" CanUserAddRows="False" 
                           ColumnHeaderStyle="{StaticResource HeaderStyle41}"    CanUserDeleteRows="False" SelectionMode="Single" VerticalAlignment="Top" AutoGenerateColumns="False"   >
                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="SupplierName" Width="180" MaxWidth="180" MinWidth="180"  Header="Supplier Name" SortMemberPath="SupplierName"  >
                            <DataGridTemplateColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition>
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=SupplierName,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}">
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock >
                                          <Hyperlink Command="{Binding DataContext.NavigateToClientCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding Sup_ID}">
                                                <TextBlock Text="{Binding SupplierName}" TextWrapping="Wrap" TextDecorations="None" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        <DataGridTextColumn x:Name="Status" Width="86" MaxWidth="86" MinWidth="86" Header="Status" Binding="{Binding Path=StatusString}" >
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>                                                
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Status,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}">
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                        </DataGridTextColumn>
                        <DataGridTemplateColumn x:Name="InvoiceNo" MinWidth="105" MaxWidth="105" Header="Invoice No."  Width="Auto"
                                                SortMemberPath="SortInvoiceNo" HeaderStyle="{StaticResource HeaderStyle41Right}">
                            
                            <DataGridTemplateColumn.HeaderTemplate>
                                <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=InvoiceNo,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}">
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                            <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" > 
                                          <Hyperlink Command="{Binding DataContext.NavigaetoPurchaseCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding InvoiceNo}">   
                                                <TextBlock HorizontalAlignment="Right" Text="{Binding InvoiceNo}" TextWrapping="Wrap" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                               
                            </DataGridTemplateColumn>
                            <DataGridTextColumn x:Name="InvoiceDate" Width="100" MaxWidth="100" MinWidth="100" Header="Invoice Date" Binding="{Binding Path=InvoiceDate}" SortMemberPath="InvoiceDateDateTime" HeaderStyle="{StaticResource HeaderStyle41Right}">
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                    <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=InvoiceDate,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}">                                        
                                                </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn x:Name="InvoiceAmount" Width="115" MaxWidth="115" MinWidth="115" Header="Invoice Amount" Binding="{Binding Path=InvoiceAmount}" HeaderStyle="{StaticResource HeaderStyle41Right}" >
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                    <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=InvoiceAmount,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"   />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="0" Grid.Row="0" />
                                            </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text=""  Grid.Row="1" Grid.Column="0"/>
                                            </Border>

                                        </Grid>
                                </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>   
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                        <DataGridTemplateColumn x:Name="CCDNo" Width="118" MaxWidth="118" MinWidth="118"
                                                SortMemberPath="DebitCashNO" HeaderStyle="{StaticResource HeaderStyle41Right}">
                            
                            <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                        <Grid.RowDefinitions>
                                                <RowDefinition ></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=CCDNo,Path=ActualWidth}" />                                           
                                        </Grid.ColumnDefinitions>
                                        <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                        <TextBlock TextWrapping="Wrap" >
                                       Cash/Cheque No
                                        </TextBlock>
                                                <Image  Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20" 
                                           HorizontalAlignment="Left" />
                                        </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}" >
                                        <TextBlock Text="Debit Note No." Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                          <Hyperlink
                                                 CommandParameter="{Binding DebitCashNO}">
                                                <TextBlock Text="{Binding DebitCashNO}" HorizontalAlignment="Right"/>
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn x:Name="CCDDate" Width="130" MaxWidth="130" MinWidth="130" Binding="{Binding Path=DebitCashDate}" SortMemberPath="CashChequeDateDate" HeaderStyle="{StaticResource HeaderStyle41Right}" >
                               
                            <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition >
                                                </RowDefinition>
                                            </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=CCDDate,Path=ActualWidth}"/>                                           
                                        </Grid.ColumnDefinitions>
                                        <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                            <TextBlock TextWrapping="Wrap">
                                        Cash/Cheque Date 
                                            </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"/>
                                        </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text="Debit Note Date" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                            <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn x:Name="CCDAmount" Width="146" MaxWidth="146" MinWidth="146" Binding="{Binding Path=DebitCashAmount}" HeaderStyle="{StaticResource HeaderStyle41Right}" >
                               
                                 <DataGridTextColumn.HeaderTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                                <RowDefinition>
                                            </RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=CCDAmount,Path=ActualWidth}"/>                                           
                                        </Grid.ColumnDefinitions>
                                        <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                        <TextBlock TextWrapping="Wrap">
                                        Cash/Cheque Amount 
                                        </TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                        </DockPanel>
                                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource HeaderLine}">
                                                <TextBlock Text="Debit Note Amount"  Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                                            </Border>
                                        </Grid>
                                </DataTemplate>
                            </DataGridTextColumn.HeaderTemplate>
                                <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="{x:Type TextBlock}">
                                        <Setter Property="HorizontalAlignment" Value="Right" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                       
                    </DataGrid>
                </Grid>
               
                <StackPanel  Orientation="Horizontal" HorizontalAlignment="left" Margin="3,0,0,0" >
                    <Grid  Width="{Binding ElementName=SupplierName,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock  Text="Total" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>

                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid  Width="{Binding ElementName=Status,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid Width="{Binding ElementName=InvoiceNo,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding ShowSelectedCount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1" >-->
                    <Grid Width="{Binding ElementName=InvoiceDate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Right" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                    <!--<Border BorderBrush="Gray" BorderThickness="1">-->
                    <Grid Width="{Binding ElementName=InvoiceAmount,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalInvoiceAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}" ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=CCDNo,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="{Binding TotalAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=CCDDate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=CCDAmount,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text="{Binding TotalCCDAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}" ></TextBlock>
                        </Border>
                    </Grid>
                    <!--</Border>-->
                </StackPanel>
            </StackPanel>
            <!--</StackPanel>
            </ScrollViewer>-->
            <StackPanel Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Orientation="Horizontal" VerticalAlignment="Bottom">
                <TextBlock   FontStyle="Italic" Foreground="Gray">
                    <Run Text="Amounts include"></Run>
                    <Run Text="{Binding TaxName}"></Run>
                </TextBlock>
                <TextBlock  Text="  Amounts are in " FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Text="{Binding CurrencyName}" FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Margin="10 0 0 0" Text="PS : Payment to Supplier" FontStyle="Italic" Foreground="Gray"></TextBlock>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="5" Grid.Row="4" HorizontalAlignment="Right"  Margin="10,10,10,0" VerticalAlignment="Bottom">
                <Button Content="New"  Margin="10 0" Style="{DynamicResource ButtonStyle}" Command="{Binding NavigaetoPurchaseCommand}" CommandParameter="New"/>
                <Button Content="Close" Command="{Binding Path=CloseCommand }"  Margin="10 0 0 0" Style="{DynamicResource ButtonStyle}"/>
            </StackPanel>
        </Grid>
    </ScrollViewer>
</UserControl>
