<UserControl x:Class="SASClient.Reports.ReportsPage.SalesQuotationReportViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SASClient.Reports.ReportsPage"
             mc:Ignorable="d" Width="1024" 
             xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_LoadedSalesQuotation">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <WindowsFormsHost>
            <rv:ReportViewer x:Name="reportViewer1"/>
        </WindowsFormsHost>
    </Grid>
</UserControl>
