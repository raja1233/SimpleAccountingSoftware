                                        <Hyperlink  Command="{Binding DataContext.NavigateToCashChequeNo, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                            >

                                            <InlineUIContainer>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding ChequeNo}" />
                                                </InlineUIContainer>
                                            <Hyperlink.CommandParameter>
                                                <MultiBinding Converter="{StaticResource MultiParamterConverter}">
                                                    <Binding Path="ChequeNo"></Binding>
                                                    <Binding Path="TransactionType"></Binding>  
                                                </MultiBinding>
                                            </Hyperlink.CommandParameter>
                                            </Hyperlink>
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TransactionNo" MinWidth="80" MaxWidth="80" Header="Date"  Width="Auto" SortMemberPath="TransactionNo"  HeaderStyle="{StaticResource HeaderStyle20Right}" CanUserSort="False">
