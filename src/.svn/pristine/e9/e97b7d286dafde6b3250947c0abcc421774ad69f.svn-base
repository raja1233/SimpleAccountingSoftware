<UserControl x:Class="SDN.Product.View.PandSDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SDN.Product.View"
             xmlns:Model="clr-namespace:SDN.UI.Entities;assembly=SDN.UI.Entities"
              xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:Con="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             mc:Ignorable="d" 
              Height="auto" Width="auto" Loaded="UserControl_Loaded"
              xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common">
    <UserControl.Resources>

        <Con:YesNoBoolConverter x:Key="YesOrNoBool"/>
        <helpers:EmptyIntConverter x:Key="EmptyIntConverter"/>
        <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
        <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="Black"/>
        </Style>
        <Style x:Key="TextBoxStyle" TargetType="{x:Type TextBox}">
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Right"></Setter>
        </Style>
        <Style x:Key="ComboBoxStyle" TargetType="{x:Type TextBox}">
            <Setter Property="Margin" Value="5"/>
        </Style>
        <Style x:Key="NALabelStyle" TargetType="{x:Type Label}">
            <Setter Property="FontSize" Value="8"></Setter>
            <Setter Property="Height" Value="23"/>
        </Style>
        <Style x:Key="RightAlignTextBox" TargetType="{x:Type TextBox}">
            <Setter Property="HorizontalContentAlignment" Value="Right"></Setter>
        </Style>
        <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
        <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
        <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
    </UserControl.Resources>
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" >

        <Grid  Margin="10,6,20,16" Width="auto" Height="auto">
            <Grid.Resources>
                <!--<Convertors:YesNoBoolConverter x:Key="YesNoBool"></Convertors:YesNoBoolConverter>-->
                <Style TargetType="{x:Type TextBox}">
                    <Setter Property="Margin" Value="0,5,0,0"/>
                    <Setter Property="Height" Value="20" />
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition  Height="0.5*"></RowDefinition>
                <RowDefinition  Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="4*"></RowDefinition>
                <RowDefinition Height="5*"></RowDefinition>
                <RowDefinition Height="auto"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition MaxWidth="500"  Width="3*" >
                </ColumnDefinition>
                <ColumnDefinition MaxWidth="580"  Width="3*"></ColumnDefinition>
                <!--<ColumnDefinition Width="1*"></ColumnDefinition>-->
            </Grid.ColumnDefinitions>

            <!--<StackPanel Grid.Row="0" Grid.Column="3"  VerticalAlignment="Top" HorizontalAlignment="Right" >
                <TextBlock Text="Last Updated" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,5"></TextBlock>
                <TextBlock Width="auto" HorizontalAlignment="Right" Height="auto"  VerticalAlignment="Top"   Margin="0,1,0,5" Text="{Binding RefreshDate}"></TextBlock>

            </StackPanel>
            <Button Grid.Row="1" Grid.Column="2" Style="{DynamicResource ButtonStyle}" Content="Refresh"   Margin="0,0,0,10" 
                    HorizontalAlignment="Right" VerticalAlignment="Center" Command="{Binding RefreshCommand}"></Button>-->

            <Grid HorizontalAlignment="Left"  Width="auto" VerticalAlignment="center" Grid.Row="1" Grid.Column="0" MaxWidth="600">
                <Grid.RowDefinitions>
                    <RowDefinition ></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="140"></ColumnDefinition>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="P&amp;S Code"  VerticalAlignment="Center" Grid.Row="0" Grid.Column="0"></TextBlock>



                <ComboBox Margin="2,5,0,5"  Grid.Row="0" Grid.Column="1" TabIndex="1" 
                          BorderBrush="{StaticResource brushWatermarkBorder}"
                          SelectedIndex="{Binding SelectedPandSID, Converter={StaticResource EmptyIntConverter}}"
                          SelectedValue="{Binding SelectedPandSID}" 
                        
                            Height="20"  Width="246" MaxWidth="250" IsEditable="True"
                      ItemsSource="{Binding ProductAndServices}" DisplayMemberPath="PSCode" SelectedValuePath="ID" Name="cmbPandSCode">

                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="SelectionChanged">
                            <i:InvokeCommandAction Command="{Binding PandSSelectionChangedCommand}"/>
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </ComboBox>
                <TextBlock Grid.Row="0" Grid.Column="1" Margin="10,5,0,5"  Style="{StaticResource WaterMarkStyle}" 
                    HorizontalAlignment="Left"   Text="Search" Foreground="{StaticResource brushWatermarkForeground}" RenderTransformOrigin="0.629,0.475" >
                    <TextBlock.Visibility>
                        <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                            <Binding ElementName="cmbPandSCode" Path="Text.IsEmpty" />
                            <Binding ElementName="cmbPandSCode" Path="IsFocused" />
                        </MultiBinding>
                    </TextBlock.Visibility>
                </TextBlock>
            </Grid>



            <Grid HorizontalAlignment="Left"  Width="auto" VerticalAlignment="center" Grid.Row="1" Grid.Column="1" Margin="20,0,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="60"></ColumnDefinition>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="P&amp;S Name"  Margin="0,9,0,9"  VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" ></TextBlock>

                <DockPanel Grid.Row="0" Grid.Column="2" Margin="10,0,0,0">
                    <ComboBox  Margin="7,0,30,5" SelectedValue="{Binding SelectedPandSID}"  TabIndex="2" VerticalAlignment="Bottom"
                     IsEditable="True" BorderBrush="{StaticResource brushWatermarkBorder}" Name="cmbPandSName" Height="20" Width="205" MaxWidth="205" ItemsSource="{Binding ProductAndServices}" DisplayMemberPath="PSName" SelectedValuePath="ID" Grid.ColumnSpan="2" >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="SelectionChanged">
                                <i:InvokeCommandAction Command="{Binding PandSSelectionChangedCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ComboBox>


                    <Button  TabIndex="3" x:Name="GoNext" Content=" &lt; "  HorizontalAlignment="Left" Command="{Binding PrevClickCommand}" Style="{DynamicResource ButtonStyle}"
                                Width="30" Height="23" VerticalAlignment="Center" Margin="5,0,0,0" Foreground="Black" Background="White" BorderBrush="Black" BorderThickness="1"/>
                    <Button  TabIndex="4" x:Name="GoBack" Content=" &gt; " Grid.Column="2" Command="{Binding NextClickCommand}" Style="{DynamicResource ButtonStyle}"
                                HorizontalAlignment="Left"  VerticalAlignment="Center" Width="30" Height="23" Margin="5,0,0,0" Foreground="Black" Background="White" BorderBrush="Black" BorderThickness="1"/>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="2" Margin="10,6,0,0" >
                    <TextBlock   Margin="17 5 0 0"   Style="{StaticResource WaterMarkStyle}" 
                    HorizontalAlignment="Left"   Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                        <TextBlock.Visibility>
                            <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                                <Binding ElementName="cmbPandSName" Path="Text.IsEmpty" />
                                <Binding ElementName="cmbPandSName" Path="IsFocused" />
                            </MultiBinding>
                        </TextBlock.Visibility>
                    </TextBlock>
                </DockPanel>
            </Grid>
            <!--<DockPanel HorizontalAlignment="Left" VerticalAlignment="center" Grid.Row="0" Grid.Column="1">
               
            </DockPanel>-->

            <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Width="350" MaxWidth="400"  Grid.Row="2" Grid.Column="0" Margin="0,13,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="140"></ColumnDefinition>
                    <ColumnDefinition Width="2*" />
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                <DockPanel Grid.Row="0" Grid.Column="0" Margin="0,0,20,0" Grid.ColumnSpan="2">
                    <TextBlock Text="Inactive" ></TextBlock>
                    <CheckBox Margin="15,0,0,0" TabIndex="5"
                              IsChecked="{Binding IsInActive,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource YesOrNoBool}}"></CheckBox>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="1"  Margin="2,0,20,0">
                    <RadioButton Name="rdnProducts" TabIndex="6" GroupName="grpType" IsChecked="{Binding IsProduct, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Checked">
                                <i:InvokeCommandAction Command="{Binding ProductsCheckedCommand}" CommandParameter="SSRT" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </RadioButton>
                    <TextBlock   >
                     <Run Text="Product"/>
                  <Run Text="*" Foreground="Red"/>
                    </TextBlock>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="2"  Margin="0,0,30,0">
                    <RadioButton Name="rdnServices" TabIndex="7" GroupName="grpType" IsChecked="{Binding IsService, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}" Height="16" VerticalAlignment="Bottom" >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Checked">
                                <i:InvokeCommandAction Command="{Binding ServiceCheckedCommand}" CommandParameter="SSRT" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </RadioButton>
                    <TextBlock>
                    <Run Text="Service"/>
                  <Run Text="*" Foreground="Red"/>
                    </TextBlock>
                </DockPanel>
            </Grid>

            <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Width="400" MaxWidth="600"  Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"
                  Margin="20,15,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="2*" />
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>

                <DockPanel Grid.Row="0" Margin="0,0,0,0" MinWidth="100" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="139">
                    <TextBlock  Text="Products ("></TextBlock>
                    <TextBlock  Text="{Binding ProductsCount}" Margin="2,1,2,1"></TextBlock>
                    <TextBlock  Text=")"></TextBlock>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="1" Margin="44,0,0,0" MinWidth="100" HorizontalAlignment="Left" Grid.ColumnSpan="2" Width="115">
                    <TextBlock  Text="Services ("></TextBlock>
                    <TextBlock  Text="{Binding ServicesCount}" Margin="2,1,2,1"></TextBlock>
                    <TextBlock  Text=")"></TextBlock>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="2" Margin="59,0,0,0" MinWidth="100" HorizontalAlignment="Left" Width="89" Grid.ColumnSpan="2">
                    <TextBlock  Text="Active ("></TextBlock>
                    <TextBlock  Text="{Binding ActivePandS}" Margin="2,1,2,1"></TextBlock>
                    <TextBlock  Text=")"></TextBlock>
                </DockPanel>
                <DockPanel Grid.Row="0" Grid.Column="3"  Margin="59,0,-71,0">
                    <TextBlock  Text="Inactive ("></TextBlock>
                    <TextBlock  Text="{Binding InActivePandS}" Margin="2,1,2,1"></TextBlock>
                    <TextBlock  Text=")"></TextBlock>
                </DockPanel>
            </Grid>



            <Grid  HorizontalAlignment="Left" Width="auto"   Grid.Row="3" Grid.Column="0" Margin="0,10,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                    <RowDefinition Height="1*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="135"></ColumnDefinition>
                    <ColumnDefinition  Width="auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Text="P&amp;S Code" >
                </TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="0" Text="P&amp;S Name">
                </TextBlock>
                <TextBlock Grid.Row="2" Grid.Column="0" Text="P&amp;S Category 1">
                </TextBlock>
                <TextBlock Grid.Row="3" Grid.Column="0" Text="P&amp;S Category 2">
                </TextBlock>
                <TextBlock Grid.Row="4" Grid.Column="0" >
                        <Run Text="{Binding TaxName}"/>
                        <Run Text="Code &amp; Rate"/>
                </TextBlock>
                <!--<TextBlock Grid.Row="5" Grid.Column="0" Text="Income Account"/>
                    <TextBlock Grid.Row="6" Grid.Column="0" Text="Costs Account"/>
                    <TextBlock Grid.Row="7" Grid.Column="0" Text="Assets Account"/>-->
                <TextBlock Grid.Row="5" Grid.Column="0" Text="P&amp;S Description"/>

                <DockPanel Grid.Row="0" Grid.Column="1" >
                    <TextBlock Text="*" Foreground="red" Margin="0,0,2,0" HorizontalAlignment="Left"></TextBlock>
                    <TextBox Name="txtPSCode" TabIndex="8" Width="246" MaxWidth="300" MaxHeight="25" Height="25" MaxLength="30" HorizontalAlignment="Left" VerticalAlignment="Top"  Text="{Binding PSCode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" ></TextBox>
                </DockPanel>
                <DockPanel Grid.Row="1" Grid.Column="1" >
                    <TextBlock Text="*" Foreground="red" Margin="0,0,2,0" HorizontalAlignment="Left"></TextBlock>
                    <TextBox Name="txtPSName" TabIndex="9" Width="246" MaxWidth="300" MaxLength="30" MaxHeight="25" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top" 
                             Text="{Binding PSName,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  >
                    </TextBox>
                </DockPanel>

                <ComboBox Name="cmbCategory1" TabIndex="10" Grid.Row="2" Grid.Column="1"   SelectedValue="{Binding DefaultPSCat1,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                     BorderBrush="Black" Height="20" Width="246" MaxWidth="300"   Margin="6,5,8,0" VerticalAlignment="Top" IsSynchronizedWithCurrentItem="True" HorizontalAlignment="Left" 
                 SelectedValuePath="ContentID" DisplayMemberPath="ContentName" ItemsSource="{Binding PSCategory1}"></ComboBox>

                <ComboBox Name="cmbCategory2"  TabIndex="11" Grid.Row="3" Grid.Column="1" SelectedValue="{Binding DefaultPSCat2,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" 
                   BorderBrush="Black"   Height="20" Width="246" MaxWidth="300"  Margin="6,5,8,0" VerticalAlignment="Top"  HorizontalAlignment="Left" IsSynchronizedWithCurrentItem="True" SelectedValuePath="ContentID" 
                          DisplayMemberPath="ContentName" ItemsSource="{Binding PSCategory2}"></ComboBox>
                <DockPanel Grid.Row="4" Grid.Column="1"  >
                    <ComboBox Height="20"  TabIndex="18"  Width="190" MaxWidth="200"   HorizontalAlignment="Left" SelectedValue="{Binding DefaultTaxID}" 
                       BorderBrush="Black"  Margin="6,5,8,0" VerticalAlignment="Top" Name="cmbTax" SelectedItem="{Binding SelectedTaxID,UpdateSourceTrigger=PropertyChanged}"
                          IsSynchronizedWithCurrentItem="True" SelectedValuePath="TaxID" DisplayMemberPath="TaxCode" ItemsSource="{Binding Tax}"  >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="SelectionChanged">
                                <i:InvokeCommandAction Command="{Binding RateSelectionChangedCommand}" CommandParameter="SSRT" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ComboBox>
                    <TextBox IsReadOnly="True" Height="25" Width="48" MaxWidth="50" HorizontalAlignment="Left"  VerticalAlignment="Top" Text="{Binding SelectedTaxID.TaxRate}">

                    </TextBox>
                </DockPanel>
                <!--<ComboBox Grid.Row="5" Grid.Column="1" Margin="0,0,5,0" Style="{DynamicResource ComboBoxStyle}"></ComboBox>
                    <ComboBox Grid.Row="6" Grid.Column="1" Margin="0,0,5,0" Style="{DynamicResource ComboBoxStyle}"></ComboBox>
                    <ComboBox Grid.Row="7" Grid.Column="1" Margin="0,0,5,0" Style="{DynamicResource ComboBoxStyle}"></ComboBox>-->
                <TextBox  TabIndex="13" Margin="0,4,8,0"  Name="txtPSDescription" Grid.Row="8" HorizontalAlignment="Left" Grid.Column="0" TextWrapping="Wrap" 
                       MaxHeight="271" MaxWidth="500" Height="271" Width="388" Grid.ColumnSpan="2" 
                          Text="{Binding PSDescription,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></TextBox>
            </Grid>

            <Grid  Grid.Column="1" Grid.Row="3" Margin="0,0,0,0" Grid.ColumnSpan="2"  Height="auto" Width="auto" 
                       Grid.RowSpan="2" HorizontalAlignment="Stretch" 
                      VerticalAlignment="Stretch">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*"></ColumnDefinition>
                    <ColumnDefinition Width="517*"/>

                </Grid.ColumnDefinitions>
                <Grid Grid.Row="0"  Name="dgPrice" MaxWidth="555" MaxHeight="150"  Width="555"  Height="auto" HorizontalAlignment="Left" 
                              Margin="10,15,16,0" Grid.ColumnSpan="2">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2.3*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>

                    </Grid.ColumnDefinitions>

                    <TextBlock Grid.Row="0" Grid.Column="0" Text=""  Margin="3"/>
                    <TextBlock Grid.Row="0" Grid.Column="1"   Margin="3">
                            <Run Text="Standard Sell Price" />
                            <Run Text="*" Foreground="Red"/>
                    </TextBlock>
                    <TextBlock Grid.Row="0" Grid.Column="2"  Margin="3" Text="Average Sell Price">
                    </TextBlock>
                    <TextBlock Grid.Row="0" Grid.Column="3"  Margin="3" Text="Last Sold Price">
                    </TextBlock>
                    <TextBlock Grid.Row="1" Grid.Column="0" Margin="3" >
                                <Run Text="Before"/>
                                 <Run Text="{Binding TaxName}"/>
                    </TextBlock>
                    <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding TaxName}"  Margin="3"/>
                    <TextBlock Grid.Row="3" Grid.Column="0"  Margin="3" >
                                 <Run Text="After"/>
                                 <Run Text="{Binding TaxName}"/>
                    </TextBlock>
                    <Border Grid.Column="0" Grid.Row="0" Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="1" Grid.Row="0"  Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="2" Grid.Row="0"  Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="0" Grid.Row="1" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="2" Grid.Row="1"  Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="0" Grid.Row="2" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="1" Grid.Row="2" Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="2" Grid.Row="2" Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="3" Grid.Row="2" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="0" Grid.Row="3" Style="{StaticResource BorderStyle}" >
                    </Border>
                    <Border Grid.Column="1" Grid.Row="3" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="2" Grid.Row="3" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="3" Grid.Row="3" Style="{StaticResource BorderStyle}">
                    </Border>


                    <TextBox TabIndex="14" Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="1" Grid.Column="1" Name="txtStandardSellpriceBeforeGST" 
                                 PreviewTextInput="DecimalTextBox_PreviewTextInput"   Text="{Binding StandardSellPriceBeforeGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" MaxLength="12" >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="LostFocus">
                                <i:InvokeCommandAction Command="{Binding SellPriceTextChangedCommand}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                    <TextBox  Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="1" Grid.Column="2" Name="txtAverageSellpriceBeforeGST" Text="{Binding AverageSellPriceBeforeGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  IsReadOnly="True" Foreground="Gray" MaxLength="15">

                    </TextBox>
                    <TextBox   Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="1" Grid.Column="3" Name="txtLastSoldpriceBeforeGST" Text="{Binding LastSoldPriceBeforeGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  IsReadOnly="True" Foreground="Gray" MaxLength="15">

                    </TextBox>
                    <TextBox Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="2" Grid.Column="1" Name="txtStandardSellpriceGST" Text="{Binding StandardSellPriceGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True" Foreground="Gray" MaxLength="15"></TextBox>
                    <TextBox Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="2" Grid.Column="2" Name="txtAverageSellpriceGST" Text="{Binding AverageSellPriceGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True" Foreground="Gray" MaxLength="15"></TextBox>
                    <TextBox Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="2" Grid.Column="3" Name="txtLastSoldPriceGST" Text="{Binding LastSoldPriceGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True" Foreground="Gray" MaxLength="15">
                    </TextBox>
                    <TextBox  Margin="3" Style="{StaticResource TextBoxStyle}" Grid.Row="3" Grid.Column="1" Name="txtStandardSellpriceAfterGST" 
                             Text="{Binding StandardSellPriceAfterGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" MaxLength="15" IsReadOnly="True" >
                        <!--<i:Interaction.Triggers>
                            <i:EventTrigger EventName="LostFocus">
                                <i:InvokeCommandAction Command="{Binding SellPriceAfterTaxTextChangedCommand}" CommandParameter="SSAT"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>-->
                    </TextBox>
                    <TextBox Style="{StaticResource TextBoxStyle}" Margin="3" Grid.Row="3" Grid.Column="2" MaxLength="15" Name="txtAverageSellpriceAfterGST" Text="{Binding AverageSellPriceAfterGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  IsReadOnly="True" Foreground="Gray">

                    </TextBox>
                    <TextBox  Style="{StaticResource TextBoxStyle}" Margin="3" Grid.Row="3" Grid.Column="3" MaxLength="15" Name="txtLastSoldPriceAfterGST" Text="{Binding LastSoldPriceAfterGST,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  IsReadOnly="True" Foreground="Gray">
                    </TextBox>
                </Grid>

                <Grid Grid.Row="1"  Name="dgPriceLast" MaxHeight="150"   MaxWidth="555"  Width="555" Height="auto" Margin="10,15,16,0" HorizontalAlignment="Left" Grid.ColumnSpan="2" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2.3*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Row="0" Grid.Column="0" Text="" Margin="3"/>
                    <TextBlock Grid.Row="0" Grid.Column="1"  Margin="3" >
                                <Run Text="Standard Cost Price"/>
                                <Run Text="*" Foreground="Red"/>
                    </TextBlock>
                    <TextBlock Grid.Row="0" Grid.Column="2" Text="Average Cost Price #" Margin="3" ></TextBlock>
                    <TextBlock Grid.Row="0" Grid.Column="3" Text="Last Purchase Price" Margin="3"></TextBlock>
                    <TextBlock Grid.Row="1" Grid.Column="0" Margin="3" >
                                  <Run Text="Before"/>
                                 <Run Text="{Binding TaxName}"/>
                    </TextBlock>
                    <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding TaxName}" Margin="3"/>
                    <TextBlock Grid.Row="3" Grid.Column="0" Margin="3">
                                 <Run Text="After"/>
                                 <Run Text="{Binding TaxName}"/>
                    </TextBlock>
                    <Border Grid.Column="0" Grid.Row="0" Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="1" Grid.Row="0"  Style="{StaticResource BorderStyle}">
                    </Border>
                    <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}">
                    </Border>
