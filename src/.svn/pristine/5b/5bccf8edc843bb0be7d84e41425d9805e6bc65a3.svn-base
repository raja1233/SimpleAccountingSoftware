<UserControl x:Class="SASClient.HomeScreen.Views.AuditTrailView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SASClient.HomeScreen.Views"
              xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
             xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             mc:Ignorable="d" Width="1024"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid >
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
                <helpers:MultipleParameterConverter x:Key="MultiParamterConverter"></helpers:MultipleParameterConverter>
                <!--<SolidColorBrush x:Key="brushWatermarkForeground" Color="LightSteelBlue" />-->
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>

            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <!--<ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>-->
            </Grid.ColumnDefinitions>

            <!--<StackPanel  Grid.Column="5" Grid.Row="0" Orientation="Vertical" HorizontalAlignment="Right" Margin="0 0 0 0">
                <Label  HorizontalAlignment="Right" Foreground="Black"  Margin="10,0,0,2" Content="Last Updated" />
                <TextBlock x:Name="LastUpdatedDate"  Margin="10,0,5,2" Grid.Row="0" HorizontalAlignment="Right"  Text="{Binding LastUpdateDate}"  />
                <Button x:Name="btnRefresh" Content="Refresh"  VerticalAlignment="Center" HorizontalAlignment="Right" Width="75"  Margin="0,4,0,4" Style="{DynamicResource ButtonStyle}" Command="{Binding RefreshCommand}"/>
            </StackPanel>-->
            <StackPanel Orientation="Horizontal" Grid.Column="0" Margin="12,22,158,6" Grid.RowSpan="2">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="0 2 10 2"   GroupName="radSelectionparameter" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All"></TextBlock>
                        <!--<TextBlock  Text="{Binding ShowAllCount}" Margin="2,1,2,1"></TextBlock>-->
                        <!--<TextBlock  Text=")"></TextBlock>-->
                    </DockPanel>
                </RadioButton>
                <!--<RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>-->
            </StackPanel>
            <Grid Grid.ColumnSpan="2" Grid.RowSpan="3" MaxWidth="300" MinWidth="300" HorizontalAlignment="Left" Margin="134,25,0,6" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="0 0 5 0"
                       Command="{Binding YearQuarterSelectedCommand}"       IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>

            </Grid>
            <Grid Grid.Column="1" Grid.ColumnSpan="3" Grid.RowSpan="3" HorizontalAlignment="Left" MinWidth="300" MaxWidth="600" Margin="214,25,0,6">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width=".25*"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <RadioButton Grid.Row="0" Grid.Column="0" Margin="20 0  5 0 " GroupName="radSelectionparameter" 
                        Command="{Binding CalendarSelectionCommand}"     IsChecked="{Binding  StartEndDateTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                <TextBlock Grid.Row="0" Grid.Column="1" Text="Start Date"></TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="End Date"></TextBlock>

                <DatePicker IsEnabled="{Binding EnableStartDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" Grid.Row="0"
                           PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"
                            Grid.Column="2" SelectedDate="{Binding SelectedSearchStartDate}" >
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox  x:Name="PART_TextBox"  IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchStartDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>

                <DatePicker IsEnabled="{Binding EnableEndDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-50 0 5 5" 
                      PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"     DisplayDateStart="{Binding EndDateValidation}" Grid.Row="1" Grid.Column="2" SelectedDate="{Binding SelectedSearchEndDate}">
                    <DatePicker.Resources>
                        <Style TargetType="{x:Type DatePickerTextBox}">
                            <Setter Property="Control.Template">
                                <Setter.Value>
                                    <ControlTemplate>
                                        <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                            <TextBox.Text >
                                                <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                    <Binding Path="SelectedDate"/>
                                                    <Binding Path="DateFormat"></Binding>
                                                    <Binding Path="DataContext.SelectedSearchEndDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                </MultiBinding>
                                            </TextBox.Text>
                                        </TextBox>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </DatePicker.Resources>
                </DatePicker>

            </Grid>
            <!--<Button Grid.Row="1" Grid.Column="5" HorizontalAlignment="Center" Style="{DynamicResource ButtonStyle}"
                    Command="{Binding SearchCommand}">Search</Button>-->
            <TextBlock  VerticalAlignment="Top"  Height="30"  Grid.Column="1" Grid.Row="3" Style="{StaticResource WaterMarkStyle}" Margin="10,10,0,0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbSuppNameSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbSuppNameSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>

            <!--<ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"  Grid.Column="1" Grid.Row="3" IsEditable="True" Margin="10,10,0,0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="150" MinWidth="150" HorizontalAlignment="Left"
                      ItemsSource="{Binding AuditTrailListcmbTranType}" 
                      DisplayMemberPath="TransactionType" SelectedValuePath="TransactionType" Name="cmbSuppNameSearch"
                      SelectedValue="{Binding SelectedTransactionType ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>-->

            <TextBlock Grid.Column="1" VerticalAlignment="Top" Grid.Row="3" Height="30"  Style="{StaticResource WaterMarkStyle}" Margin="10,10,0,0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbQuotNoSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbQuotNoSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="3" IsEditable="True" HorizontalAlignment="Left" Margin="10,10,0,0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="200" MinWidth="200"
                      ItemsSource="{Binding AuditTrailListcmbName}" 
                      DisplayMemberPath="Name" SelectedValuePath="Name" Name="cmbQuotNoSearch"
                      SelectedValue="{Binding SelectedName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>

            <TextBlock Grid.Column="2" VerticalAlignment="Top"  Style="{StaticResource WaterMarkStyle}"  Grid.Row="3" Height="30"  Margin="-40 10 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbTranNo" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbTranNo" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"  Grid.Column="2" Grid.Row="3"  IsEditable="True" HorizontalAlignment="Left" Margin="-45 10 0 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="130" MinWidth="130"
                      ItemsSource="{Binding AuditTrailListcmbTranNo}" 
                      DisplayMemberPath="TransactionNo" SelectedValuePath="TransactionNo" Name="cmbTranNo"
                      SelectedValue="{Binding SelectedTransactionNo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>
            <TextBlock Grid.Column="2" VerticalAlignment="Top"  Style="{StaticResource WaterMarkStyle}"  Grid.Row="3" Height="30" Margin="90 10 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbDate" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbDate" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"  Grid.Column="2" Grid.Row="3"  IsEditable="True" HorizontalAlignment="Left" Margin="84,10,0,0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="100" MinWidth="100"
                      ItemsSource="{Binding AuditTrailListcmbDate}" 
                      DisplayMemberPath="TransactionDateStr" SelectedValuePath="TransactionDateStr" Name="cmbDate"
                      SelectedValue="{Binding SelectedTranDate ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>
            <TextBlock VerticalAlignment="Top"  Style="{StaticResource WaterMarkStyle}"  Grid.Row="3" Grid.Column="3" Height="30" Margin="-17 13 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbAmount" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbAmount" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.ColumnSpan="2"  Grid.Column="3" Grid.Row="3"  IsEditable="True" HorizontalAlignment="Left" Margin="-20 10 0 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="100" MinWidth="100"
                      ItemsSource="{Binding AuditTrailListcmbAmount}" 
                      DisplayMemberPath="AmountStr" SelectedValuePath="AmountStr" Name="cmbAmount"
                      SelectedValue="{Binding SelectedAmountStr ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>
            <TextBlock VerticalAlignment="Top"  Style="{StaticResource WaterMarkStyle}"  Grid.Row="3" Grid.Column="3" Height="30" Margin="85 15 0 0"  Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbUserName" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbUserName" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"   Grid.Column="3" Grid.Row="3"  IsEditable="True" HorizontalAlignment="Left" Margin="78 10 0 0" MaxHeight="30" VerticalAlignment="Top" MaxWidth="150" MinWidth="150"
                      ItemsSource="{Binding AuditTrailListcmbUserName}" 
                      DisplayMemberPath="UserName" SelectedValuePath="UserName" Name="cmbUserName"
                      SelectedValue="{Binding SelectedCreditNoteNo ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>

            <StackPanel   
                         Grid.Row="3" Grid.Column="0"  Margin="10,40,10,0">
                <Grid>
                    <DataGrid x:Name="CustomGridLines1" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="180"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="8" IsTextSearchEnabled="True" SelectedIndex="0"   GridLinesVisibility="All" IsReadOnly="True" BorderThickness="1" 
                  HorizontalScrollBarVisibility="Disabled" SelectedValue="{Binding SelectedFormName,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Name" MinHeight="100"  Height="{Binding SIGridHeight}"  x:Name="dg2" ItemsSource="{Binding FormList}" CanUserAddRows="False" 
                  ColumnHeaderStyle="{StaticResource HeaderStyle20}"  Background="Transparent"  CanUserDeleteRows="False" SelectionMode="Single" VerticalAlignment="Top" AutoGenerateColumns="False"  >

                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="FormName" Width="250" MaxWidth="250" Header="Transaction Type" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid Width="182">
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <!--<ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>-->
                                                <ColumnDefinition Width="{Binding ElementName=TransactionType,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                            </DockPanel>

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                                  <TextBlock Text="{Binding Name}" /></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                        </DataGrid.Columns>
                        <DataGrid.Resources>
                            <Style TargetType="DataGridRow">
                                <Style.Resources>
                                    <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Black"/>
                                </Style.Resources>
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.CellStyle>
                            <Style TargetType="DataGridCell">
                                <Style.Triggers>
                                    <Trigger Property="IsSelected"
                                                     Value="True">
                                        <Setter Property="BorderBrush"
                                                 Value="Black" />
                                        <Setter Property="Background"
                                                    Value="Black" />
                                        <Setter Property="Foreground"
                                                 Value="White" />

                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.CellStyle>
                        <DataGrid.RowStyle>
                            <Style TargetType="DataGridRow">
                                <Style.Triggers>
                                    <Trigger Property="IsSelected"
                                                     Value="True">
                                        <Setter Property="BorderBrush"
                                                 Value="Black" />
                                        <Setter Property="Background"
                                                    Value="Black" />
                                        <Setter Property="Foreground"
                                                 Value="White" />

                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.RowStyle>
                    </DataGrid>
                </Grid>

            </StackPanel>
            <!--<ScrollViewer VerticalScrollBarVisibility="Auto"  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" Margin="-10,30,0,0">-->
            <StackPanel   
                         Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3" Margin="10,40,10,0">
                <Grid>
                    <DataGrid x:Name="CustomGridLines" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0"  >
                        <DataGrid.Columns>
                            <!--<DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="150"></DataGridTextColumn>-->
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="200"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col2}"  Width="130"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col3}"  Width="100"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="150"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="150"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="8" IsTextSearchEnabled="True"   GridLinesVisibility="All" 
                  HorizontalScrollBarVisibility="Disabled" MinHeight="100"  Height="{Binding SIGridHeight}"  BorderThickness="1" x:Name="dg1" ItemsSource="{Binding AuditTrailList}" CanUserAddRows="False" 
                  ColumnHeaderStyle="{StaticResource HeaderStyle20}"  Background="Transparent"  CanUserDeleteRows="False" SelectionMode="Single" VerticalAlignment="Top" AutoGenerateColumns="False"  >
                        <DataGrid.Columns>
                            <!--<DataGridTemplateColumn x:Name="TransactionType" Width="150" MaxWidth="150" Header="TransactionType" SortMemberPath="TransactionType" >
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid Width="182">
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                -->
                            <!--<ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>-->
                            <!--
                                                <ColumnDefinition Width="{Binding ElementName=TransactionType,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                            </DockPanel>

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                                  <TextBlock Text="{Binding TransactionType}" /></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>-->
                            <DataGridTemplateColumn
                                x:Name="Name" MinWidth="250"  MaxWidth="200" Header="Cus/Sup/P&amp;S/Acc Name"  Width="Auto" SortMemberPath="Name">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Name,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0">
                                                <TextBlock Text="{TemplateBinding Content}"  HorizontalAlignment="Right"/>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Left" ><Hyperlink Command="{Binding DataContext.NavigatetoNameCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                ><InlineUIContainer>   
                                                  <TextBlock HorizontalAlignment="Left" Text="{Binding Name}" />
                                              </InlineUIContainer>
                                        <Hyperlink.CommandParameter>
                                            <MultiBinding Converter="{StaticResource MultiParamterConverter}">
                                                 <Binding Path="CustomerID"/>
                                                 <Binding Path="TransactionType"/>
                                             </MultiBinding>
                                        </Hyperlink.CommandParameter></Hyperlink></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="TransactionNo" MinWidth="130" MaxWidth="130" Header="Transaction No."  Width="Auto" SortMemberPath="TransactionNo"  HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=TransactionNo,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}" HorizontalAlignment="Right" />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1" />
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                            <Hyperlink Style="{StaticResource HyperlinkDataGrid}" Command="{Binding DataContext.NavigaetoTranCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"  IsEnabled="{Binding NoHyperLink}">
                                            <InlineUIContainer>   
                                                  <TextBlock HorizontalAlignment="Right" Text="{Binding TransactionNo}" />
                                              </InlineUIContainer>
                                                
                                        <Hyperlink.CommandParameter>
                                            <MultiBinding Converter="{StaticResource MultiParamterConverter}">
                                                 <Binding Path="TransactionNo"/>
                                                 <Binding Path="ScreenNo"/>
                                             </MultiBinding>
                                        </Hyperlink.CommandParameter></Hyperlink></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="DateTime" MinWidth="100" MaxWidth="100" Header="Date"  Width="Auto" SortMemberPath="TransactionDate" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=DateTime,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding TransactionDateStr}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Amount" MinWidth="100" MaxWidth="150" Header="Amount"  Width="Auto" SortMemberPath="Amount" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=Amount,Path=ActualWidth}"/>
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right">
                                                <TextBlock Text="{TemplateBinding Content}"  />
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                            </DockPanel>

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right" Text="{Binding AmountStr}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>

                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="UserName" Width="150" MaxWidth="150"  SortMemberPath="UserName">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="{Binding ElementName=UserName,Path=ActualWidth}" />
                                            </Grid.ColumnDefinitions>
                                            <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left">
                                                <TextBlock TextWrapping="Wrap"><Run Text="User Name"/></TextBlock>
                                                <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"  />
                                            </DockPanel>

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right"><Hyperlink  Command="{Binding DataContext.NavigaetoCreditNoteCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding CreditCashNO}"><InlineUIContainer>
                                                  <TextBlock Text="{Binding CreditCashNO}" />
                                              </InlineUIContainer></Hyperlink></TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>

            </StackPanel>
            <!--</ScrollViewer>-->
            <StackPanel Margin="10 0 0 0" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Orientation="Horizontal" VerticalAlignment="Bottom">
                <TextBlock   FontStyle="Italic" Foreground="Gray">
                    <Run Text="Amounts include "></Run>
                    <Run Text="{Binding TaxName}"/>
                </TextBlock>
                <TextBlock  Text="  Amounts are in  " FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Text="{Binding CurrencyName}" FontStyle="Italic" Foreground="Gray"></TextBlock>

            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="4" Grid.Row="5" HorizontalAlignment="Right"  Margin="10,10,10,0" VerticalAlignment="Bottom">
                <!--<Button Content="New"  Margin="10 0" Style="{DynamicResource ButtonStyle}" Command="{Binding NavigaetoSalesCommand}" CommandParameter="New"/>-->
                <Button Content="Close"  Command="{Binding CloseCommand }"  Margin="10 0 0 0" Style="{DynamicResource ButtonStyle}"/>
            </StackPanel>
        </Grid>
    </ScrollViewer>
</UserControl>
