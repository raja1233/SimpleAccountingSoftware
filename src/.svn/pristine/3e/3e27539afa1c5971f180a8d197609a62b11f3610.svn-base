<UserControl x:Class="SDN.Product.View.PandSPurchaseView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SDN.Product.View"
             xmlns:helpers="clr-namespace:SDN.Common.Converter;assembly=SDN.Common"
              xmlns:my="clr-namespace:SDN.Common;assembly=SDN.Common"
             mc:Ignorable="d" 
             Width="1024" HorizontalAlignment="Left"
             d:DesignHeight="300" d:DesignWidth="300" Background="White">
    <ScrollViewer  HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid>
            <Grid.Resources>
                <helpers:TextInputToVisibilityConverter x:Key="TextInputToVisibilityConverter"/>
                <helpers:DateTimeConverter x:Key="DateTimeFormatter"></helpers:DateTimeConverter>
                <!--<SolidColorBrush x:Key="brushWatermarkForeground" Color="LightSteelBlue" />-->
                <Style x:Key="BorderStyle" TargetType="{x:Type Border}">
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="BorderBrush" Value="Black"/>
                </Style>
                <!--Watermark style start here-->
                <SolidColorBrush x:Key="brushWatermarkBackground" Color="White" />
                <SolidColorBrush x:Key="brushWatermarkForeground" Color="Gray" />
                <SolidColorBrush x:Key="brushWatermarkBorder" Color="Black" />
                <!--Watermark style End here-->
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="69"></RowDefinition>
                <RowDefinition Height="0"></RowDefinition>
                <RowDefinition Height="0"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
                <ColumnDefinition Width="256"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.ColumnSpan="6" Grid.Row="0" Margin="10 10 0 0">
                <RadioButton x:Name="radShowAllYes" Command="{Binding ShowAllCommand}"  Margin="0 2 10 2"   GroupName="radSelectionparameter" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowAllTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show All "></TextBlock>
                        <!--<TextBlock  Text="{Binding AllCount}" Margin="2,1,2,1"></TextBlock>-->
                        <TextBlock  Text=""></TextBlock>
                    </DockPanel>
                </RadioButton>
                <!--<RadioButton x:Name="radShowAllNo"  Margin="5 2 10 2" Command="{Binding ShowSelectedCommand}"   GroupName="radShowAll" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowSelectedTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Show Selected ("></TextBlock>
                        <TextBlock  Text="{Binding ShowSelectedCount}" Margin="2,1,2,1"></TextBlock>
                        <TextBlock  Text=")"></TextBlock>
                    </DockPanel>
                </RadioButton>-->

                <DockPanel Grid.Column="0" Grid.ColumnSpan="4" Grid.RowSpan="2" Grid.Row="1"
                        MaxWidth="600" MinWidth="600" HorizontalAlignment="Left" Margin="10 0 0 0">
                    <Grid   MaxWidth="300" MinWidth="300" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width=".25*"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <RadioButton Grid.Row="0" Grid.Column="0" x:Name="radyearmonthYes" GroupName="radSelectionparameter" Margin="0 0 5 0"
                     Command="{Binding YearQuarterSelectedCommand}"   IsChecked="{Binding  YearmonthQuartTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                        <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Year"></TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Quater"></TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Month"></TextBlock>
                        <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="1" IsEnabled="{Binding EnableYearDropDown}" IsEditable="True" 
                          ItemsSource="{Binding YearRange}" 
                      DisplayMemberPath="Year" SelectedValuePath="Year" Name="cmbYearSearch"
                      SelectedValue="{Binding SelectedSearchYear ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                        <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="2" IsEnabled="{Binding EnableQuarterDropDown}" IsEditable="True" IsTextSearchEnabled="True"     
                      IsTextSearchCaseSensitive="False"     
                      StaysOpenOnEdit="True"    
                      TextSearch.TextPath="{Binding Quarter}"
                          ItemsSource="{Binding QuarterList}" 
                      DisplayMemberPath="Quarter" SelectedValuePath="ID" Name="cmbQuarterSearch"
                      SelectedValue="{Binding SelectedSearchQuarter ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                        <ComboBox BorderBrush="Black" Grid.Row="1" Grid.Column="3" IsEnabled="{Binding EnableMonthDropDown}" IsEditable="True"
                          ItemsSource="{Binding MonthList}" 
                      DisplayMemberPath="Month" SelectedValuePath="ID" Name="cmbMonthSearch"
                      SelectedValue="{Binding SelectedSearchMonth ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></ComboBox>
                        <Border Grid.Column="1" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                        <Border Grid.Column="1" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                        <Border Grid.Column="2" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                        <Border Grid.Column="2" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                        <Border Grid.Column="3" Grid.Row="0" Style="{StaticResource BorderStyle}"></Border>
                        <Border Grid.Column="3" Grid.Row="1" Style="{StaticResource BorderStyle}"></Border>
                    </Grid>
                    <Grid  MaxWidth="300" MinWidth="300" Margin="10,0,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width=".30*"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <RadioButton Grid.Row="0" Grid.Column="0" Margin="20 0  5 0 " GroupName="radSelectionparameter" 
                      Command="{Binding CalendarSelectionCommand}"       IsChecked="{Binding  StartEndDateTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}"></RadioButton>
                        <TextBlock Grid.Row="0" Grid.Column="1" Text="Start Date"></TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="1" Text="End Date"></TextBlock>
                        <DatePicker IsEnabled="{Binding StartEndDateTrue}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-60 0 5 5" Grid.Row="0"
                            PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"
                            Grid.Column="2" SelectedDate="{Binding SelectedSearchStartDate}"
                            >
                            <DatePicker.Resources>
                                <Style TargetType="{x:Type DatePickerTextBox}">
                                    <Setter Property="Control.Template">
                                        <Setter.Value>
                                            <ControlTemplate>
                                                <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                                    <TextBox.Text >
                                                        <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                            <Binding Path="SelectedDate"/>
                                                            <Binding Path="DateFormat"></Binding>
                                                            <Binding Path="DataContext.SelectedSearchStartDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                        </MultiBinding>
                                                    </TextBox.Text>
                                                </TextBox>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </DatePicker.Resources>
                        </DatePicker>
                        <DatePicker IsEnabled="{Binding EnableEndDropDown}" HorizontalAlignment="Left" MaxWidth="150" MinWidth="150" Margin="-60 0 5 5" 
                      PreviewKeyUp="OpenDatePicker_KeyDown"  PreviewKeyDown="OpenDatePicker_KeyUp"     DisplayDateStart="{Binding EndDateValidation}" Grid.Row="1" Grid.Column="2" SelectedDate="{Binding SelectedSearchEndDate}">
                            <DatePicker.Resources>
                                <Style TargetType="{x:Type DatePickerTextBox}">
                                    <Setter Property="Control.Template">
                                        <Setter.Value>
                                            <ControlTemplate>
                                                <TextBox x:Name="PART_TextBox" IsReadOnly="True" BorderThickness="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"  BorderBrush="{DynamicResource BaseBorderBrush}" >
                                                    <TextBox.Text >
                                                        <MultiBinding Converter="{StaticResource DateTimeFormatter}">
                                                            <Binding Path="SelectedDate"/>
                                                            <Binding Path="DateFormat"></Binding>
                                                            <Binding Path="DataContext.SelectedSearchEndDate"
                                                                RelativeSource="{RelativeSource AncestorType={x:Type DatePicker}}" 
                                                                 />
                                                        </MultiBinding>
                                                    </TextBox.Text>
                                                </TextBox>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </DatePicker.Resources>
                        </DatePicker>

                    </Grid>

                </DockPanel>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="2" Margin="10 10 0 0">
                <RadioButton x:Name="radProductsYes"   Margin="0 2 10 2" Command="{Binding ShowProductCommand}"  GroupName="radShowBoth" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding  ShowProducts, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Products"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radServicesYes"  Margin="5 2 10 2" Command="{Binding ShowServiceCommand}"   GroupName="radShowBoth" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowServices, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Services"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radShowNo"  Margin="5 2 10 2" Command="{Binding ShowBothTrueCommand}"   GroupName="radShowBoth" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding ShowBoth, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Both"></TextBlock>
                    </DockPanel>
                </RadioButton>
            </StackPanel>
            <!--<StackPanel Orientation="Horizontal" Grid.Column="1"  Grid.Row="3" Margin="0 10 0 0">
                <RadioButton x:Name="radSummaryYes" Command="{Binding ShowSummaryCommand}" Checked="radSummaryYes_Checked"    Margin="0 2 10 2"   GroupName="radSummary" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding IsSummaryTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Summary"></TextBlock>
                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radDetailsyes"  Margin="5 2 10 2" Command="{Binding ShowDetailCommand}" Checked="radDetailsyes_Checked"   GroupName="radSummary" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding IsSummaryFalse, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="Details"></TextBlock>
                    </DockPanel>
                </RadioButton>
            </StackPanel>-->
            <StackPanel Orientation="Horizontal" Grid.Column="3"  Grid.Row="3" Margin="85,10,15,0">
                <RadioButton x:Name="radExcgstYes" Command="{Binding ShowExcludingTaxCommand}"    Margin="0 2 10 2"   GroupName="radTax" TabIndex="14" HorizontalAlignment="Left" IsChecked="{Binding IncludingGSTFalse, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="exc. "></TextBlock>
                        <TextBlock  Text="{Binding TaxName}" Margin="2,1,2,1"></TextBlock>

                    </DockPanel>
                </RadioButton>
                <RadioButton x:Name="radIncgstyes"  Margin="5 2 10 2" Command="{Binding ShowIncludingTaxCommand}"    GroupName="radTax" TabIndex="15"  HorizontalAlignment="Left"  IsChecked="{Binding IncludingGSTTrue, BindingGroupName=Group1, UpdateSourceTrigger=PropertyChanged}">
                    <DockPanel>
                        <TextBlock  Text="inc. "></TextBlock>
                        <TextBlock  Text="{Binding TaxName}" Margin="2,1,2,1"></TextBlock>

                    </DockPanel>
                </RadioButton>
            </StackPanel>
            <!--<StackPanel Orientation="Horizontal" Grid.Column="2"  Grid.Row="3" Margin="-50 10 0 0">
                <TextBlock Text="P&amp;S Category 1" Margin="0 0 0 0"></TextBlock>
                <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"  IsEditable="True" Margin="12 0 0 0" HorizontalAlignment="Left" MaxHeight="30" VerticalAlignment="Top" MaxWidth="160" MinWidth="160"
                      ItemsSource="{Binding PandSSoldListCode}" SelectedIndex="0"
                      DisplayMemberPath="PandSCode" SelectedValuePath="PandSCode" Name="cmbCat1Search"
                      SelectedValue="{Binding SelectedPandSCode ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

                </ComboBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Column="3"  Grid.Row="3" Margin="-30 10 0 0" HorizontalAlignment="Right">
                <TextBlock Text="P&amp;S Category 2" Margin="0 0 0 0"></TextBlock>
                <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}"  IsEditable="True" Margin="12 0 15 0" HorizontalAlignment="Left" MaxHeight="30" VerticalAlignment="Top" MaxWidth="160" MinWidth="160"
                      ItemsSource="{Binding PandSSoldListCode}" SelectedIndex="0"
                      DisplayMemberPath="PandSCode" SelectedValuePath="PandSCode" Name="cmbCat2Search"
                      SelectedValue="{Binding SelectedPandSCode ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

                </ComboBox>
            </StackPanel>-->

            <ComboBox Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="0" Grid.Row="4" IsEditable="True" Margin="12 10 0 0" HorizontalAlignment="Left" MaxHeight="30" VerticalAlignment="Top" MaxWidth="218" MinWidth="218"
                      ItemsSource="{Binding PandSPurchaseListCode}" 
                      DisplayMemberPath="PandSCode" SelectedValuePath="PandSCode" Name="cmbSuppNameSearch"
                      SelectedValue="{Binding SelectedPandSCode ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">

            </ComboBox>
            <TextBlock Grid.Column="0" Grid.Row="4"  Margin="13,10,0,0"   Style="{StaticResource WaterMarkStyle}" 
                    HorizontalAlignment="Left"   Text="Search" Foreground="{StaticResource brushWatermarkForeground}" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbSuppNameSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbSuppNameSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>

            <ComboBox  Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="2"
                       IsEditable="True" HorizontalAlignment="Left"  Margin="230 10 0 0" MaxHeight="30" VerticalAlignment="Top" 
                       MaxWidth="216" MinWidth="216" ItemsSource="{Binding PandSPurchaseListName}" 
                      DisplayMemberPath="PandSName" SelectedValuePath="PandSName" Name="cmbQuotNoSearch"
                      SelectedValue="{Binding SelectedPandSName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>

            <TextBlock Grid.Column="0" Grid.Row="4"  Margin="235,10,0,0"   Style="{StaticResource WaterMarkStyle}" 
                    HorizontalAlignment="Left"   Text="Search" Foreground="{StaticResource brushWatermarkForeground}" Grid.ColumnSpan="2" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbQuotNoSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbQuotNoSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <ComboBox  Background="Transparent" BorderBrush="{StaticResource brushWatermarkBorder}" Grid.Column="1" Grid.Row="4" Grid.ColumnSpan="2"
                       IsEditable="True" HorizontalAlignment="Left"  Margin="191,10,0,0" MaxHeight="30" VerticalAlignment="Top" 
                       MaxWidth="148" MinWidth="148" ItemsSource="{Binding PandSPurchaseListCus}" 
                      DisplayMemberPath="SupplierName" SelectedValuePath="SupplierName" Name="cmbSupplierNameSearch"
                      SelectedValue="{Binding SelectedSupplierName ,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            </ComboBox>
            <TextBlock Grid.Column="1" Grid.Row="4"  Margin="192,10,0,0"  Style="{StaticResource WaterMarkStyle}" 
                    HorizontalAlignment="Left"   Text="Search" Foreground="{StaticResource brushWatermarkForeground}" Grid.ColumnSpan="2" >
                <TextBlock.Visibility>
                    <MultiBinding Converter="{StaticResource TextInputToVisibilityConverter}">
                        <Binding ElementName="cmbSupplierNameSearch" Path="Text.IsEmpty" />
                        <Binding ElementName="cmbSupplierNameSearch" Path="IsFocused" />
                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>


            <StackPanel  Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="4" Margin="0,40,0,0" Width="998" >
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20*"/>
                        <ColumnDefinition Width="26*"/>
                        <ColumnDefinition Width="41*"/>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="882*"/>
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="CustomGridLines" ItemsSource="{Binding BackGroundLines}" Grid.Column="0" 
                          Height="{Binding ElementName=dg1,Path=ActualHeight}" GridLinesVisibility="All"   Width="{Binding ElementName=dg1,Path=ActualWidth}"
                          HorizontalGridLinesBrush="LightGray" VerticalGridLinesBrush="LightGray" AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled"
                          ColumnHeaderStyle="{StaticResource BackHeaderStyle20}" HeadersVisibility="All" Margin="0,0,0,0" Grid.ColumnSpan="5"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="" Binding="{Binding Col0}"  Width="215"></DataGridTextColumn>
                            <DataGridTextColumn Header="" Binding="{Binding Col1}"  Width="215"></DataGridTextColumn>

                            <DataGridTextColumn Header="" Binding="{Binding Col4}"  Width="150"></DataGridTextColumn>
                            <DataGridTextColumn x:Name="qtybackground" Header="" Binding="{Binding Col4}"  Width="50"></DataGridTextColumn>
                            <DataGridTextColumn x:Name="pricebackground" Header="" Binding="{Binding Col4}"  Width="95"></DataGridTextColumn>
                            <DataGridTextColumn x:Name="amtbackground" Header="" Binding="{Binding Col4}"  Width="95"></DataGridTextColumn>
                            <DataGridTextColumn x:Name="invoicenobackground" Header="" Binding="{Binding Col2}"  Width="80"></DataGridTextColumn>
                            <DataGridTextColumn x:Name="datebackground" Header="" Binding="{Binding Col3}"  Width="82"></DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid Grid.ColumnSpan="5" MinHeight="100" Height="{Binding PandSPurchaseGridHeight}" IsTextSearchEnabled="True" GridLinesVisibility="All"  BorderThickness="1" x:Name="dg1" ItemsSource="{Binding PandSPurchaseList}" CanUserAddRows="False"  CanUserDeleteRows="False" CanUserResizeColumns="False"
                    ColumnHeaderStyle="{StaticResource HeaderStyle20}"    HorizontalScrollBarVisibility="Disabled" Background="Transparent"  CanUserReorderColumns="False" SelectionMode="Single"  VerticalAlignment="Top" AutoGenerateColumns="False" >
                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="PandSCode" MinWidth="215" MaxWidth="215" Header="Product &amp; Service Code" Width="Auto" SortMemberPath="PandSCode">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock>
                                          <Hyperlink Command="{Binding DataContext.NavigateToProductCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding PandSCode}">
                                                <TextBlock Text="{Binding PandSCode}" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="PandSName" MinWidth="215" MaxWidth="215" Header="Product &amp; Service Name" SortMemberPath="PandSName"  Width="Auto">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"/>
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Left">
                                          <Hyperlink Command="{Binding DataContext.NavigateToProductCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding PandSName}">
                                                <TextBlock Text="{Binding PandSName}" />
                                           </Hyperlink>
                                       </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="SupplierName" MinWidth="150" MaxWidth="150" Header="Supplier Name" SortMemberPath="SupplierName">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Left">
                                          <Hyperlink Command="{Binding DataContext.NavigateToSupplierCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding SupplierId}">
                                                <TextBlock Text="{Binding SupplierName}" TextWrapping="Wrap" HorizontalAlignment="Left"/>
                                           </Hyperlink>
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Quantity" MinWidth="50"  Header="Qty" SortMemberPath="Qty" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                          
                                                <TextBlock Text="{Binding QtyStr}" TextWrapping="Wrap" HorizontalAlignment="Right"/>
                                           
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Price" MinWidth="95"  Header="Price" SortMemberPath="Price" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                         
                                                <TextBlock Text="{Binding PriceStr}" TextWrapping="Wrap" HorizontalAlignment="Right"/>
                                         
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="Amount" MinWidth="95"  Header="Amount" SortMemberPath="Amount" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                       
                                                <TextBlock Text="{Binding AmountStr}" TextWrapping="Wrap" HorizontalAlignment="Right"/>
                                     
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceNo" Width="80" MaxWidth="80" Header="Invoice No" SortMemberPath="InvoiceNo" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>

                                            <TextBlock TextWrapping="Wrap" HorizontalAlignment="Right" >
                                       Invoice No
                                            </TextBlock>
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />

                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                          <Hyperlink Command="{Binding DataContext.NavigateToInvoiceCommand, RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                                 CommandParameter="{Binding InvoiceNo}">
                                                <TextBlock Text="{Binding InvoiceNo}" TextWrapping="Wrap" HorizontalAlignment="Left"/>
                                           </Hyperlink>
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="InvoiceDate" Width="82" MaxWidth="82" Header="Invoice Date" SortMemberPath="InvoiceDate" HeaderStyle="{StaticResource HeaderStyle20Right}">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition  Width="10"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="{TemplateBinding Content}" HorizontalAlignment="Right"  />
                                            <Image Source="/SASClient;component/Images/Shorting-Arrow-2.png"
                                                Width="10" Height="20"
                                           HorizontalAlignment="Left"
                                           Grid.Column="1"
                                   />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock HorizontalAlignment="Right">
                                         
                                                <TextBlock Text="{Binding InvoiceDate}" TextWrapping="Wrap" HorizontalAlignment="Left"/>
                                         
                                            </TextBlock>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
                <StackPanel  Orientation="Horizontal" HorizontalAlignment="left" Margin="0,1,0,0" Height="20" >
                    <Grid  Width="{Binding ElementName=PandSCode,Path=ActualWidth}" >
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock  Text="Total" FontWeight="Bold"></TextBlock>
                        </Border>
                    </Grid>
                    <Grid  Width="{Binding ElementName=PandSName,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=SupplierName,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=Quantity,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleLeft}">
                            <TextBlock Text="" ></TextBlock>
                            <!--<TextBlock Text="{Binding TotalQuantity,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"></TextBlock>-->
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=Price,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text="" ></TextBlock>
                            <!--<TextBlock Text="{Binding TotalPrice,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>-->
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=Amount,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text="{Binding TotalAmount,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DisplayTotalStyle}"  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=InvoiceNo,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                    <Grid Width="{Binding ElementName=InvoiceDate,Path=ActualWidth}">
                        <Border Style="{StaticResource BorderStyleRight}">
                            <TextBlock Text=""  ></TextBlock>
                        </Border>
                    </Grid>
                </StackPanel>
            </StackPanel>

            <StackPanel Margin="10 0 0 0" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                <TextBlock  Text="  Amounts are in  " FontStyle="Italic" Foreground="Gray"></TextBlock>
                <TextBlock Text="{Binding CurrencyName}" FontStyle="Italic" Foreground="Gray"></TextBlock>
            </StackPanel>
            <StackPanel Orientation="Horizontal"   Grid.Column="3"   Grid.Row="5" HorizontalAlignment="Right"  Margin="10,0,0,0" VerticalAlignment="Bottom">
                <Button Content="Export"  Margin="10 0" Style="{DynamicResource ButtonStyle}" Command="{Binding ExportCommand}" CommandParameter="New"/>

                <Button Content="Close"  Margin="10 0" Style="{DynamicResource ButtonStyle}" Command="{Binding NavigaetoSalesCommand}" CommandParameter="New"/>
            </StackPanel>
        </Grid>
    </ScrollViewer>
</UserControl>

